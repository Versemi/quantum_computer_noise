Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["H", "1"], "=", 
    RowBox[{"V", "=", 
     RowBox[{"e", " ", "z", " ", "\[CurlyEpsilon]"}]}]}], TraditionalForm]],
  ExpressionUUID->"6b6cf77b-a507-4186-9547-9566a1f7c05c"],
 "\n\nBy the Stark Effect, ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox[
      SubscriptBox["E", "1"], 
      RowBox[{"(", "1", ")"}]], "=", 
     RowBox[{
      RowBox[{"e", " ", "\[CurlyEpsilon]", 
       TemplateBox[{RowBox[{"1", ",", "0", ",", "0"}]},
        "Bra"], "z", 
       TemplateBox[{RowBox[{"1", ",", "0", ",", "0"}]},
        "Ket"]}], "=", "0"}]}], ";", " ", 
    RowBox[{
     SuperscriptBox[
      SubscriptBox["E", "1"], 
      RowBox[{"(", "2", ")"}]], "=", 
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{
         UnderscriptBox["\[Sum]", 
          RowBox[{
           RowBox[{"n", "\[NotEqual]", "1"}], ",", "l", ",", "m"}]], 
         FractionBox[
          RowBox[{
           RowBox[{
            SuperscriptBox["e", "2"], 
            SuperscriptBox["\[CurlyEpsilon]", "2"]}], "|", 
           RowBox[{
            TemplateBox[{RowBox[{"n", ",", "l", ",", "m"}]},
             "Bra"], "z", 
            TemplateBox[{RowBox[{"1", ",", "0", ",", "0"}]},
             "Ket"]}], 
           SuperscriptBox["|", "2"]}], 
          RowBox[{
           SuperscriptBox["E1", 
            RowBox[{"(", "0", ")"}]], "-", 
           SuperscriptBox[
            SubscriptBox["E", "n"], 
            RowBox[{"(", "0", ")"}]]}]]}], "..."}], "..."}], "..."}], 
     RowBox[{"(*", 
      RowBox[{"1", ")"}]}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "fe670950-6f03-4ec2-8369-694518a7a57b"],
 "\nThe radial matrix element is ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Integral]", 
     RowBox[{
      RowBox[{"\[DifferentialD]", "r"}], " ", 
      RowBox[{
       SubscriptBox["P", 
        RowBox[{"5", "s"}]], "(", "r", ")"}], " ", "r", " ", 
      RowBox[{
       SubscriptBox["P", 
        RowBox[{"5", "p"}]], "(", "r", ")"}]}]}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"5.1", "..."}], "..."}], "..."}], 
    RowBox[{"(*", 
     RowBox[{"2", ")"}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "16af3655-e1f4-4aef-a96b-9cde70a4af93"],
 "\nHence, ",
 Cell[BoxData[
  FormBox[
   RowBox[{"(*", 
    RowBox[{"2", ")"}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "a3bcc580-3aa7-46db-9d1f-ca7631230d3d"],
 " has ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     TemplateBox[{RowBox[{"5", ",", "1", ",", "0"}]},
      "Bra"], 
     SubscriptBox["H", "1"], 
     TemplateBox[{RowBox[{"5", ",", "0", ",", "0"}]},
      "Ket"]}], "=", 
    RowBox[{
     SuperscriptBox["e", "2"], 
     SuperscriptBox["\[CurlyEpsilon]", "2"], 
     RowBox[{
      SubsuperscriptBox["\[Integral]", "0", "\[Infinity]"], 
      RowBox[{
       SuperscriptBox["r", "2"], 
       RowBox[{"\[DifferentialD]", "r"}], 
       RowBox[{
        SubsuperscriptBox["\[Integral]", "0", "\[Pi]"], 
        RowBox[{
         RowBox[{"Sin", "[", "\[Theta]", "]"}], 
         RowBox[{"\[DifferentialD]", "\[Theta]"}], 
         RowBox[{
          SubsuperscriptBox["\[Integral]", "0", 
           RowBox[{"2", "\[Pi]"}]], 
          RowBox[{
           RowBox[{"\[DifferentialD]", "\[Phi]"}], " ", 
           SuperscriptBox[
            SubscriptBox["R", 
             RowBox[{"5", ",", "1"}]], "\[Conjugate]"], 
           SuperscriptBox[
            SubscriptBox["Y", 
             RowBox[{"5", ",", "0"}]], "\[Conjugate]"], "r", " ", 
           RowBox[{"Cos", "[", "\[Theta]", "]"}], 
           SuperscriptBox[
            SubscriptBox["R", 
             RowBox[{"5", ",", "0"}]], "\[Conjugate]"], 
           SubscriptBox["Y", 
            RowBox[{"5", ",", "0"}]]}]}]}]}]}]}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "57f52e99-03a5-42e9-a55e-57ab3d1bd543"],
 " \n",
 Cell[BoxData[
  FormBox[
   RowBox[{"(*", 
    RowBox[{"1", ")"}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "64cae41b-6ace-422f-8f00-b8d8f387e3de"],
 " has ",
 Cell[BoxData[
