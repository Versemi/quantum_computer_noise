Cell[CellGroupData[{

Cell["2) T 11.11", "Section",
 CellChangeTimes->{
  3.730415839062394*^9, {3.730416184065913*^9, 3.7304161842938814`*^9}, {
   3.7305015897472024`*^9, 
   3.7305015898713913`*^9}},ExpressionUUID->"267bbac8-78ff-4aaa-b3a0-\
21e463980b77"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox[
      OverscriptBox["H", "^"], "0"], "=", 
     RowBox[{
      FractionBox[
       SuperscriptBox[
        SubscriptBox[
         OverscriptBox["p", "^"], "x"], "2"], 
       RowBox[{"2", "m"}]], "+", 
      RowBox[{
       FractionBox["1", "2"], "m", " ", 
       SuperscriptBox["\[Omega]", "2"], 
       SuperscriptBox[
        OverscriptBox["x", "^"], "2"]}], "+", 
      FractionBox[
       SuperscriptBox[
        SubscriptBox[
         OverscriptBox["p", "^"], "y"], "2"], 
       RowBox[{"2", " ", "m"}]], "+", 
      RowBox[{
       FractionBox["1", "2"], "m", " ", 
       SuperscriptBox["\[Omega]", "2"], 
       SuperscriptBox[
        OverscriptBox["y", "^"], "2"]}]}]}], ";", " ", 
    RowBox[{
     SubscriptBox[
      OverscriptBox["H", "^"], "1"], "=", 
     RowBox[{"b", " ", 
      OverscriptBox["x", "^"], 
      OverscriptBox["y", "^"], " "}]}]}], TraditionalForm]],ExpressionUUID->
  "cf8924f0-fabd-49bd-aa98-d8655aea7538"],
 "\nThe first-order energy shifts to the ground state is given by ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     TemplateBox[{SuperscriptBox[
        SubscriptBox["\[CurlyPhi]", "n"], "0"]},
      "Bra"], 
     SubscriptBox[
      OverscriptBox["H", "^"], "1"], 
     TemplateBox[{SuperscriptBox[
        SubscriptBox["\[CurlyPhi]", "n"], "0"]},
      "Ket"]}], "=", 
    RowBox[{
     TemplateBox[{SuperscriptBox[
        SubscriptBox["\[CurlyPhi]", "n"], "0"]},
      "Bra"], "b", " ", 
     OverscriptBox["x", "^"], 
     OverscriptBox["y", "^"], 
     TemplateBox[{SuperscriptBox[
        SubscriptBox["\[CurlyPhi]", "n"], "0"]},
      "Ket"]}]}], TraditionalForm]],ExpressionUUID->
  "69ea615e-9250-424b-9e07-390187906c4b"],
 "\n\nGiven by (7.11), ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     OverscriptBox["x", "^"], "=", 
     RowBox[{
      SqrtBox[
       FractionBox["\[HBar]", 
        RowBox[{"2", " ", "m", " ", 
         SubscriptBox["\[Omega]", "x"]}]]], 
      RowBox[{"(", 
       RowBox[{
        OverscriptBox["a", "^"], "+", 
        SuperscriptBox[
         OverscriptBox["a", "^"], "\[Dagger]"]}], ")"}]}]}], ";", " ", 
    RowBox[{
     OverscriptBox["y", "^"], "=", 
     RowBox[{
      SqrtBox[
       FractionBox["\[HBar]", 
        RowBox[{"2", " ", "m", " ", 
         SubscriptBox["\[Omega]", "y"]}]]], 
      RowBox[{"(", 
       RowBox[{
        OverscriptBox["a", "^"], "+", 
        SuperscriptBox[
         OverscriptBox["a", "^"], "\[Dagger]"]}], ")"}]}]}], ";", 
    RowBox[{
     SubscriptBox[
      OverscriptBox["p", "^"], "x"], "=", 
     RowBox[{
      RowBox[{"-", "\[ImaginaryI]"}], 
      SqrtBox[
       FractionBox[
        RowBox[{"m", " ", 
         SubscriptBox["\[Omega]", "x"], " ", "\[HBar]"}], "2"]], 
      RowBox[{"(", 
       RowBox[{
        OverscriptBox["a", "^"], "-", 
        SuperscriptBox[
         OverscriptBox["a", "^"], "\[Dagger]"]}], ")"}]}]}], ";", 
    RowBox[{
     SubscriptBox[
      OverscriptBox["p", "^"], "y"], "=", 
     RowBox[{
      RowBox[{"-", "\[ImaginaryI]"}], 
      SqrtBox[
       FractionBox[
        RowBox[{"m", " ", 
         SubscriptBox["\[Omega]", "y"], " ", "\[HBar]"}], "2"]], 
      RowBox[{"(", 
       RowBox[{
        OverscriptBox["a", "^"], "-", 
        SuperscriptBox[
         OverscriptBox["a", "^"], "\[Dagger]"]}], ")"}]}]}]}], 
   TraditionalForm]],ExpressionUUID->"ba7971b9-2410-4b14-a99e-2f7b71ee952a"],
 " \n\nTo obtain the eigenvectors and eigenvalues of ",
 Cell[BoxData[
  FormBox[
   SubscriptBox[
    OverscriptBox["H", "^"], "0"], TraditionalForm]],ExpressionUUID->
  "edeba4bf-3b5e-4202-8cd0-cb961a246c52"],
 ":\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox[
      OverscriptBox["H", "^"], "0"], 
