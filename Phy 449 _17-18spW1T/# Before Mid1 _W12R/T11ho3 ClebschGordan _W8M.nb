   RowBox[{
    SubsuperscriptBox["C", 
     RowBox[{
      SubscriptBox["j", "1"], 
      SubscriptBox["m", "1"], 
      SubscriptBox["j", "2"], 
      SubscriptBox["m", "2"]}], 
     RowBox[{
      SubscriptBox["j", "3"], 
      SubscriptBox["m", "3"]}]], "=", 
    RowBox[{
     SqrtBox[
      FractionBox[
       RowBox[{
        RowBox[{"2", 
