       RowBox[{
        RowBox[{"2", " ", 
         SubscriptBox["j", "1"]}], "+", "1"}]]], 
     SubsuperscriptBox["C", 
      RowBox[{
       RowBox[{
        SubscriptBox["j", "3"], 
        SubscriptBox["m", "3"], 
        SubscriptBox["j", "2"]}], "-", 
       SubscriptBox["m", "2"]}], 
      RowBox[{
       SubscriptBox["j", "1"], "-", 
