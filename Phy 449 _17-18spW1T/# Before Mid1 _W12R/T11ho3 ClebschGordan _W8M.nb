Cell[TextData[{
 "In this equation we have introduced the spherical components ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["v", "0"], "=", 
    SubscriptBox["v", "z"]}], TraditionalForm]],ExpressionUUID->
  "edc2ed53-e5fd-4b31-b98b-b568a6f27937"],
 ", ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox[
     OverscriptBox["v", "^"], "1"], "=", 
    RowBox[{
     FractionBox[
      RowBox[{"-", "1"}], 
      SqrtBox["2"]], 
     RowBox[{"(", 
      RowBox[{
       SubscriptBox["v", "x"], "+", 
       RowBox[{"\[ImaginaryI]", " ", 
        SubscriptBox["v", "y"]}]}], ")"}]}]}], TraditionalForm]],
  ExpressionUUID->"49c66f2f-619e-44cb-a5bc-38f572e17ccf"],
