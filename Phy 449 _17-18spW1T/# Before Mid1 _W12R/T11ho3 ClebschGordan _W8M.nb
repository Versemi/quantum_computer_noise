Cell[BoxData[Cell[TextData[Cell[BoxData[
  FormBox[Cell[TextData[Cell[BoxData[
    FormBox[
     RowBox[{
      TemplateBox[{RowBox[{
          SubscriptBox["j", "1"], 
          SubscriptBox["j", "2"], "j", "\[VeryThinSpace] ", "m"}]},
       "Ket"], "=", 
      RowBox[{
       RowBox[{
        SubscriptBox["\[Sum]", 
         RowBox[{
          SubscriptBox["m", "1"], 
          SubscriptBox["m", "2"]}]], 
        RowBox[{
         TemplateBox[{RowBox[{
             RowBox[{
               SubscriptBox["j", "1"], 
               SubscriptBox["m", "1"]}], ";", 
             RowBox[{
               SubscriptBox["j", "2"], 
               SubscriptBox["m", "2"]}]}]},
          "Ket"], 
         TemplateBox[{RowBox[{
             RowBox[{
               SubscriptBox["j", "1"], 
               SubscriptBox["m", "1"]}], ";", 
             RowBox[{
               SubscriptBox["j", "2"], 
               SubscriptBox["m", "2"]}]}],RowBox[{"j", " ", "m"}]},
          "BraKet"]}]}], "=", 
       RowBox[{
        SubscriptBox["\[Sum]", 
         RowBox[{
          SubscriptBox["m", "1"], 
          SubscriptBox["m", "2"]}]], 
        RowBox[{
         TemplateBox[{RowBox[{
             SubscriptBox["m", "1"], ";", 
             SubscriptBox["m", "2"]}]},
          "Ket"], 
         SubsuperscriptBox["C", 
          RowBox[{
           SubscriptBox["j", "1"], 
           SubscriptBox["m", "1"], 
           SubscriptBox["j", "2"], 
           SubscriptBox["m", "2"]}], 
          RowBox[{"j", " ", "m"}]]}]}]}]}], TraditionalForm]],
