Cell[TextData[{
 "There is another way to calculate this.  It invokes the following argument: \
since the fine-structure splitting is proportional to ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[LeftAngleBracket]", 
     RowBox[{"S", "\[CenterDot]", "L"}], "\[RightAngleBracket]"}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"\[LeftAngleBracket]", 
       RowBox[{"S", "\[CenterDot]", 
        RowBox[{"(", 
         RowBox[{"J", "-", "S"}], ")"}]}], "\[RightAngleBracket]"}], 
      "\[Rule]", 
      RowBox[{"\[LeftAngleBracket]", 
       RowBox[{"S", "\[CenterDot]", "J"}], "\[RightAngleBracket]"}]}], "=", 
     " ", 
     RowBox[{
      RowBox[{"\[LeftAngleBracket]", 
       RowBox[{"S", "\[CenterDot]", "L"}], "\[RightAngleBracket]"}], "+", " ", 
      RowBox[{"\[LeftAngleBracket]", 
       RowBox[{"S", "\[CenterDot]", "S"}], "\[RightAngleBracket]"}]}]}]}], 
   TraditionalForm]],ExpressionUUID->"0b4e4e2b-ad45-451c-8e1b-07dba1c55180"],
 ".  Since  ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"\[LeftAngleBracket]", 
      RowBox[{"S", "\[CenterDot]", "L"}], "\[RightAngleBracket]"}], " ", 
     "&"}], " ", 
    RowBox[{"\[LeftAngleBracket]", 
     RowBox[{"S", "\[CenterDot]", "S"}], "\[RightAngleBracket]"}], " "}], 
   TraditionalForm]],ExpressionUUID->"7dfaecf0-9147-49ef-8e2b-ec6b1eb0c8bf"],
 " are independent of ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["m", "j"], TraditionalForm]],ExpressionUUID->
  "a5514b98-bed0-4a08-9390-db7c7790f9af"],
 ", this in turn implies that ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[LeftAngleBracket]", 
    RowBox[{"S", "\[CenterDot]", "J"}], "\[RightAngleBracket]"}], 
   TraditionalForm]],ExpressionUUID->"bee12c1f-e9c3-4335-b1da-32f336cea750"],
 " is independent of ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["m", "j"], TraditionalForm]],ExpressionUUID->
  "066282bd-8572-4a86-9b72-20fc7f3eda0b"],
 ", so ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[LeftAngleBracket]", "S", "\[RightAngleBracket]"}], 
    "\[Proportional]", 
    RowBox[{"\[LeftAngleBracket]", "J", "\[RightAngleBracket]"}]}], 
   TraditionalForm]],ExpressionUUID->"c46207ad-3be6-4d01-b860-a767c145e57b"],
 " and furthermore ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[LeftAngleBracket]", 
     SubscriptBox["S", "z"], "\[RightAngleBracket]"}], "=", 
    RowBox[{
     RowBox[{
      FractionBox[
       RowBox[{
        RowBox[{"\[LeftAngleBracket]", 
         RowBox[{"S", "\[CenterDot]", "J"}], "\[RightAngleBracket]"}], " "}], 
       
       RowBox[{
        RowBox[{"\[LeftAngleBracket]", 
         RowBox[{"J", "\[CenterDot]", "J"}], "\[RightAngleBracket]"}], " "}]], 
      RowBox[{"\[LeftAngleBracket]", 
       SubscriptBox["J", "z"], "\[RightAngleBracket]"}]}], "=", 
     RowBox[{
      FractionBox[
       RowBox[{
        RowBox[{"\[LeftAngleBracket]", 
         RowBox[{"S", "\[CenterDot]", "L"}], "\[RightAngleBracket]"}], "+", 
        RowBox[{"\[LeftAngleBracket]", 
         SuperscriptBox["S", "2"], "\[RightAngleBracket]"}], " "}], 
       RowBox[{"\[LeftAngleBracket]", 
        SuperscriptBox["J", "2"], "\[RightAngleBracket]"}]], 
      RowBox[{"\[LeftAngleBracket]", 
       SubscriptBox["J", "z"], "\[RightAngleBracket]"}]}]}]}], 
   TraditionalForm]],ExpressionUUID->"07918022-9f27-4395-bea8-0565598952cd"],
 ".  By a similar argument,  ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[LeftAngleBracket]", 
     SubscriptBox["L", "z"], "\[RightAngleBracket]"}], "=", 
    RowBox[{
     FractionBox[
      RowBox[{
       RowBox[{"\[LeftAngleBracket]", 
        RowBox[{"S", "\[CenterDot]", "L"}], "\[RightAngleBracket]"}], "+", 
       RowBox[{"\[LeftAngleBracket]", 
        SuperscriptBox["L", "2"], "\[RightAngleBracket]"}], " "}], 
      RowBox[{"\[LeftAngleBracket]", 
       SuperscriptBox["J", "2"], "\[RightAngleBracket]"}]], 
     RowBox[{"\[LeftAngleBracket]", 
      SubscriptBox["J", "z"], "\[RightAngleBracket]"}]}]}], TraditionalForm]],
  ExpressionUUID->"76cd92cb-b020-4fd7-b851-0b2b94a2efb5"],
 ".  So"
}], "Text",
