  3.729260114596375*^9, 
  3.729260121883069*^9}},ExpressionUUID->"b800fe00-31fb-47f0-806a-\
179e1dc4fc16"],

Cell[BoxData[
 FormBox[Cell[TextData[Cell[BoxData[
   FormBox[
    RowBox[{
     TemplateBox[{SubsuperscriptBox["\[CurlyPhi]", "g", "1"]},
      "Ket"], "=", 
     RowBox[{
      RowBox[{
       TemplateBox[{SubsuperscriptBox["\[CurlyPhi]", "g", "0"]},
        "Ket"], "+", 
       RowBox[{
        UnderscriptBox["\[Sum]", 
         RowBox[{"k", "\[NotEqual]", "g"}]], 
        RowBox[{
         TemplateBox[{SubsuperscriptBox["\[CurlyPhi]", "k", "0"]},
          "Ket"], 
         FractionBox[
          RowBox[{
           TemplateBox[{SubsuperscriptBox["\[CurlyPhi]", "k", "0"]},
            "Bra"], 
           StyleBox["V",
            FontSize->12], 
           TemplateBox[{SubsuperscriptBox["\[CurlyPhi]", "g", "0"]},
            "Ket"]}], 
          StyleBox[
           RowBox[{
            SubsuperscriptBox["E", "g", "0"], "-", 
            SubsuperscriptBox["E", "k", "0"]}],
           FontSize->12]]}]}]}], "=", 
      RowBox[{
       TemplateBox[{SubsuperscriptBox["\[CurlyPhi]", "g", "0"]},
        "Ket"], "-", 
       RowBox[{
        UnderscriptBox["\[Sum]", 
         RowBox[{"k", "\[NotEqual]", "g"}]], 
        RowBox[{
         TemplateBox[{SubsuperscriptBox["\[CurlyPhi]", "k", "0"]},
          "Ket"], 
         FractionBox[
          SubscriptBox["V", 
           RowBox[{"k", "\[VeryThinSpace]", "g"}]], 
          StyleBox[
           SubscriptBox["E", 
            RowBox[{"k", "\[VeryThinSpace]", "g"}]],
           FontSize->12]]}]}]}]}]}], TraditionalForm]],ExpressionUUID->
   "b451b473-8349-40ce-aeb2-c7536bd16af0"]],ExpressionUUID->
   "43767d96-7262-4ed1-9041-1aef66e64584"], 
  TraditionalForm]], "DisplayFormulaNumbered",
