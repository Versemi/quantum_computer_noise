Cell[BoxData[Cell[TextData[{
  Cell[BoxData[{
   FormBox[
    RowBox[{
     SubsuperscriptBox["E", 
      SubscriptBox["m", "j"], "1"], "=", 
     RowBox[{
      TemplateBox[{SubsuperscriptBox["\[CurlyPhi]", "g", "0"]},
       "Bra"], "V", 
      TemplateBox[{SubsuperscriptBox["\[CurlyPhi]", "g", "0"]},
       "Ket"]}]}], TraditionalForm], "\[IndentingNewLine]", 
   FormBox[
    RowBox[{"=", 
     RowBox[{
      UnderscriptBox["\[Sum]", 
       RowBox[{
        SubsuperscriptBox["m", "l", "'"], 
        SubsuperscriptBox["m", "s", "'"]}]], 
      RowBox[{
       SubsuperscriptBox["C", 
        RowBox[{"1", "\[VeryThinSpace]", 
         SubsuperscriptBox["m", "l", "'"], 
         RowBox[{"1", "/", "2"}], "\[VeryThinSpace]", 
         SubsuperscriptBox["m", "s", "'"]}], 
        RowBox[{
         RowBox[{"3", "/", "2"}], " ", 
         SubscriptBox["m", "j"]}]], 
       RowBox[{
        TemplateBox[{RowBox[{
            SubsuperscriptBox["m", "l", "'"], 
            SubsuperscriptBox["m", "s", "'"]}]},
         "Bra"], "(", 
        RowBox[{
         SubscriptBox["\[Mu]", "B"], 
         RowBox[{"B", "(", 
          RowBox[{
           SubscriptBox["L", "z"], "+", 
           RowBox[{"2", " ", 
            SubscriptBox["S", "z"]}]}], ")"}]}], ")"}], 
       RowBox[{
        UnderscriptBox["\[Sum]", 
         RowBox[{
          SubscriptBox["m", "l"], 
          SubscriptBox["m", "s"]}]], 
        RowBox[{
         SubsuperscriptBox["C", 
          RowBox[{"1", "\[VeryThinSpace]", 
           SubscriptBox["m", "l"], 
           RowBox[{"1", "/", "2"}], "\[VeryThinSpace]", 
           SubscriptBox["m", "s"]}], 
          RowBox[{
           RowBox[{"3", "/", "2"}], " ", 
           SubscriptBox["m", "j"]}]], 
         TemplateBox[{RowBox[{
             SubscriptBox["m", "l"], 
             SubscriptBox["m", "s"]}]},
          "Ket"]}]}]}]}]}], TraditionalForm]}],ExpressionUUID->
   "753b7705-2d54-4e14-8604-543677d36dac"],
  "\n",
  Cell[BoxData[{
   FormBox[
    RowBox[{"=", 
     RowBox[{
      SubscriptBox["\[Mu]", "B"], "B", 
      RowBox[{
       SubscriptBox["\[Sum]", 
        RowBox[{
         SubscriptBox["m", "l"], 
         SubscriptBox["m", "s"]}]], 
       RowBox[{
        SuperscriptBox[
         RowBox[{"(", 
          SubsuperscriptBox["C", 
           RowBox[{"1", "\[VeryThinSpace]", 
            SubscriptBox["m", "l"], 
            RowBox[{"1", "/", "2"}], "\[VeryThinSpace]", 
            SubscriptBox["m", "s"]}], 
           RowBox[{
            RowBox[{"3", "/", "2"}], " ", 
            SubscriptBox["m", "j"]}]], ")"}], "2"], 
        RowBox[{
         TemplateBox[{RowBox[{
             SubscriptBox["m", "l"], 
             SubscriptBox["m", "s"]}]},
          "Bra"], "(", 
         RowBox[{
          SubscriptBox["L", "z"], "+", 
          RowBox[{"2", " ", 
           SubscriptBox["S", "z"]}]}], ")"}], 
        TemplateBox[{RowBox[{
            SubscriptBox["m", "l"], 
            SubscriptBox["m", "s"]}]},
         "Ket"]}]}]}]}], TraditionalForm], "\[IndentingNewLine]", 
   FormBox[
    RowBox[{"=", 
     RowBox[{
      SubscriptBox["\[Mu]", "B"], "B", 
      RowBox[{
       SubscriptBox["\[Sum]", 
        RowBox[{
         SubscriptBox["m", "l"], 
         SubscriptBox["m", "s"]}]], 
       RowBox[{
        SuperscriptBox[
         RowBox[{"(", 
          SubsuperscriptBox["C", 
           RowBox[{"1", "\[VeryThinSpace]", 
            SubscriptBox["m", "l"], 
            RowBox[{"1", "/", "2"}], "\[VeryThinSpace]", 
            SubscriptBox["m", "s"]}], 
           RowBox[{
            RowBox[{"3", "/", "2"}], " ", 
            SubscriptBox["m", "j"]}]], ")"}], "2"], 
        RowBox[{"(", 
         RowBox[{
          SubscriptBox["m", "l"], "+", 
          RowBox[{"2", 
           SubscriptBox["m", "s"]}]}], ")"}]}]}]}]}], TraditionalForm], 
  "\[IndentingNewLine]", 
   FormBox[
    RowBox[{"=", 
     RowBox[{
      SubscriptBox["\[Mu]", "B"], "B", " ", 
      SubscriptBox["m", "j"], 
      RowBox[{
       SubscriptBox["\[Sum]", 
        SubscriptBox["m", "s"]], 
       RowBox[{
        SuperscriptBox[
         RowBox[{"(", 
          SubsuperscriptBox["C", 
           RowBox[{
            RowBox[{"1", "\[VeryThinSpace]", 
             SubscriptBox["m", "j"]}], "-", 
            RowBox[{
             SubscriptBox["m", "s"], 
             RowBox[{"1", "/", "2"}], "\[VeryThinSpace]", 
             SubscriptBox["m", "s"]}]}], 
           RowBox[{
            RowBox[{"3", "/", "2"}], " ", 
            SubscriptBox["m", "j"]}]], ")"}], "2"], 
        RowBox[{"(", 
         RowBox[{"1", "+", 
          FractionBox[
           SubscriptBox["m", "s"], 
           SubscriptBox["m", "j"]]}], ")"}]}]}]}]}], TraditionalForm]}],
   ExpressionUUID->"b1cb432b-23cb-4c48-b7c6-b5f6017341d8"]
 }],ExpressionUUID->
