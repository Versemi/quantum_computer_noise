Cell[TextData[{
 "Since ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"[", 
      RowBox[{
       SubscriptBox["S", "z"], ",", 
       SuperscriptBox["F", "2"]}], "]"}], "\[NotEqual]", "0"}], ","}], 
   TraditionalForm]],ExpressionUUID->"a743eda4-d13a-4f85-8416-effecd65da01"],
