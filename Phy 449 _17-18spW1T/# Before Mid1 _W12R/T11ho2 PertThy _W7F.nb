Cell[TextData[{
 "The vector ",
 Cell[BoxData[
  FormBox[
   TemplateBox[{SubscriptBox["\[Chi]", "g"]},
    "Ket"], TraditionalForm]],ExpressionUUID->
  "7754dfe2-3115-4523-b67d-f32d968bc17a"],
 " tells us ",
 StyleBox["how different the actual eigenvector is from the original one",
  Background->RGBColor[1, 1, 0.85]],
 ", so we are free to pick ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    TemplateBox[{SubscriptBox["\[Chi]", "g"],SuperscriptBox[
       SubscriptBox["\[CurlyPhi]", "g"], "0"]},
     "BraKet"], "=", "0"}], TraditionalForm]],
  CellChangeTimes->{{3.601978038367998*^9, 3.6019780483126698`*^9}, {
   3.601978082009653*^9, 3.601978098808195*^9}, {3.601978141103197*^9, 
