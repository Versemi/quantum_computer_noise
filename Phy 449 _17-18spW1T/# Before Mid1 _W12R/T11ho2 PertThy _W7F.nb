     FractionBox[
      RowBox[{
       RowBox[{
        FractionBox["3", "2"], 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"j", "(", 
           RowBox[{"j", "+", "1"}], ")"}], "-", 
          RowBox[{"l", "(", 
           RowBox[{"l", "+", "1"}], ")"}], "-", 
          RowBox[{"s", "(", 
           RowBox[{"s", "+", "1"}], ")"}]}], ")"}]}], "+", 
