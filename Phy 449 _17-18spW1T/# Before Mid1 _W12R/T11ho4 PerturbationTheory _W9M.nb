Cell[BoxData[Cell[TextData[Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["V", 
      RowBox[{"k", "\[VeryThinSpace]", "g"}]], "+", 
     RowBox[{
      SuperscriptBox["H", 
       RowBox[{" ", 
        RowBox[{"k", " ", 
         SuperscriptBox["k", "\[Prime]"]}]}]], 
      SubscriptBox["\[Chi]", "g"]}]}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{
       SubscriptBox["E", "g"], 
       SubscriptBox["\[Chi]", "g"]}], "\[Rule]", " ", 
      SubscriptBox["\[Chi]", "g"]}], "=", 
     RowBox[{
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{
         SubscriptBox["E", "g"], "-", 
         SuperscriptBox["H", 
          RowBox[{" ", 
           RowBox[{"k", " ", 
            SuperscriptBox["k", "\[Prime]"]}]}]]}], ")"}], 
       RowBox[{"-", "1"}]], 
      SuperscriptBox["V", 
       RowBox[{"k", "\[VeryThinSpace]", "g"}]]}]}]}], TraditionalForm]],
  ExpressionUUID->"89d1f789-bbcf-4fc2-97f7-66bff3df9bd8"]],ExpressionUUID->
