Cell[TextData[{
 "Degenerate perturbation theory tells us we don\[CloseCurlyQuote]t have to \
worry about admixtures of the other np states into the Hamiltonian.  The \
Hamiltonian commutes with ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["J", "z"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "d2b51cb7-bbfa-4b95-8d61-60f8dddea9a2"],
 ", so our states can be labelled by ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["m", "j"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "af8e1572-22af-4697-b9e1-e0867cc0171d"],
 ", but the Zeeman part of the Hamiltonian has non-zero matrix elements \
between states ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    TemplateBox[{RowBox[{
        FractionBox["3", "2"], 
        SubscriptBox["m", "j"]}]},
     "Ket"], " ", "and", " ", 
    TemplateBox[{RowBox[{
        FractionBox["1", "2"], 
        SubscriptBox["m", "j"]}]},
     "Ket"], " "}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "209b2815-c217-4466-b6a3-cea4d5cbfd24"],
 ":"
}], "Text",
 CellChangeTimes->{{3.729863057910623*^9, 3.7298630934602537`*^9}, {
  3.729863175192307*^9, 
  3.729863274902698*^9}},ExpressionUUID->"ba1bdce9-f9a8-4da2-82f2-\
67db31905fd2"],

Cell[BoxData[Cell[TextData[Cell[BoxData[
  FormBox[Cell[TextData[{
    " ",
    Cell[BoxData[
     FormBox[Cell[TextData[Cell[BoxData[
       FormBox[
        RowBox[{
         RowBox[{
          RowBox[{
           TemplateBox[{RowBox[{
               FractionBox["1", "2"], 
               SubscriptBox["m", "j"]}]},
            "Bra"], 
           SubscriptBox["L", "z"]}], "+", 
          RowBox[{"2", " ", 
           SubscriptBox["S", "z"], 
           TemplateBox[{RowBox[{
               FractionBox["3", "2"], 
               SubscriptBox["m", "j"]}]},
            "Ket"]}]}], "=", 
         RowBox[{
          SubscriptBox["\[Sum]", 
           RowBox[{
            SubscriptBox["m", "l"], 
            SubscriptBox["m", "s"]}]], 
          RowBox[{
           RowBox[{
            SubsuperscriptBox["C", 
             RowBox[{"1", " ", 
              SubscriptBox["m", "l"], 
              FractionBox["1", "2"], 
              SubscriptBox["m", "s"]}], 
             RowBox[{
              RowBox[{"1", "/", "2"}], " ", 
              SubscriptBox["m", "j"]}]], "(", 
            RowBox[{
             SubscriptBox["m", "l"], "+", 
             RowBox[{"2", 
              SubscriptBox["m", "s"]}]}], ")"}], 
           SubsuperscriptBox["C", 
            RowBox[{"1", " ", 
             SubscriptBox["m", "l"], 
             FractionBox["1", "2"], 
             SubscriptBox["m", "s"]}], 
            RowBox[{
             RowBox[{"3", "/", "2"}], " ", 
             SubscriptBox["m", "j"]}]]}]}]}], TraditionalForm]],
       FormatType->"TraditionalForm",ExpressionUUID->
       "b7a13c6c-e1bd-4b7c-a594-cbf1e542fc47"]],ExpressionUUID->
       "1247fe65-8957-4d6c-bde1-7098642d14f5"], TraditionalForm]],
     FormatType->"TraditionalForm",ExpressionUUID->
     "d96efb10-f39c-4181-bad2-b4cbfa22bc96"]
   }],ExpressionUUID->"ac7a79c2-b6a1-421f-a5ff-1226187fd5d1"], 
   TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
