Cell[TextData[{
 "Suppose we know the ",
 Cell[BoxData[
  FormBox["N", TraditionalForm]],ExpressionUUID->
  "6b1a13a9-8c25-4a87-ae5c-9c506aa0f42d"],
 " eigenstates of an \[OpenCurlyDoubleQuote]unperturbed\
\[CloseCurlyDoubleQuote] Hamiltonian ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["H", "0"], TraditionalForm]],ExpressionUUID->
  "2956deb0-7484-4749-b1d7-93762e5b4aa4"],
 ".  Focus on a single one of the energy eigenstates ",
 Cell[BoxData[
  FormBox[
   TemplateBox[{SubsuperscriptBox["\[CurlyPhi]", "g", "0"]},
    "Ket"], TraditionalForm]],ExpressionUUID->
  "96b51839-128d-4a0f-8f1e-fd91268a90ef"],
 ", which for now we ",
 StyleBox["assume has no other eigenstates degenerate with it",
  FontVariations->{"Underline"->True}],
 ".  Now we add a small \[OpenCurlyDoubleQuote]perturbation\
\[CloseCurlyDoubleQuote] ",
 Cell[BoxData[
  FormBox["V", TraditionalForm]],ExpressionUUID->
  "69b7adbc-27f4-4f16-8c52-d8f9dbd6cf0d"],
 ":  ",
 Cell[BoxData[
  FormBox[
   RowBox[{"H", "=", 
    RowBox[{
     SuperscriptBox["H", "0"], "+", "V"}]}], TraditionalForm]],
  ExpressionUUID->"8e833c4e-50f6-4df6-89a2-277bd1a32c2d"],
 ".  We ",
 StyleBox["want to know how ",
  FontVariations->{"Underline"->True}],
 Cell[BoxData[
  FormBox[
   SubsuperscriptBox["E", "m", "0"], TraditionalForm]],
  FontVariations->{"Underline"->True},ExpressionUUID->
  "57609b49-eb41-438e-b3d3-36ddba87662a"],
 StyleBox[" and ",
  FontVariations->{"Underline"->True}],
 Cell[BoxData[
  FormBox[
   TemplateBox[{SubsuperscriptBox["\[CurlyPhi]", "g", "0"]},
    "Ket"], TraditionalForm]],
  FontVariations->{"Underline"->True},ExpressionUUID->
  "8dc5fd45-3027-4b33-8d42-8b22ff454201"],
 StyleBox[" change when we add ",
  FontVariations->{"Underline"->True}],
 Cell[BoxData[
  FormBox["V", TraditionalForm]],
  FontVariations->{"Underline"->True},ExpressionUUID->
  "130469a7-75db-4796-8ef5-2615b52a7462"],
 "."
}], "Text",
 CellChangeTimes->{{3.725619978322093*^9, 3.7256202653636312`*^9}, {
  3.7256204673065643`*^9, 3.725620480253628*^9}, {3.725620800160327*^9, 
