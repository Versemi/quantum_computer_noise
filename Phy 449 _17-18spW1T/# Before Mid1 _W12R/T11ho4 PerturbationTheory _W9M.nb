Cell[TextData[{
 "The Hamiltonian matrix for the ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["m", "j"], "=", 
    RowBox[{"1", "/", "2", " "}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "cbb06ab1-4575-42bf-81db-65ba1e3fba7a"],
 "states becomes (using ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox[
     RowBox[{"\[LeftAngleBracket]", 
      RowBox[{"L", "\[CenterDot]", "S"}], "\[RightAngleBracket]"}], "j"], "=", 
    RowBox[{"\[Piecewise]", GridBox[{
       {
        RowBox[{"1", "/", "2"}], 
        RowBox[{"j", "\[Equal]", 
         RowBox[{"3", "/", "2"}]}]},
       {
        RowBox[{"-", "1"}], 
        RowBox[{"j", "\[Equal]", 
         RowBox[{"1", "/", "2"}]}]}
      }]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "e06b77be-82e4-40b3-b2e1-7e384af29ca9"],
 ")"
}], "Text",
