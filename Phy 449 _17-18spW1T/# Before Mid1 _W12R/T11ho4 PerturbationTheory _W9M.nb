  FormBox[
   RowBox[{
    RowBox[{
     SubsuperscriptBox["E", "g", "1"], "+", 
     RowBox[{
      SuperscriptBox["V", 
       RowBox[{"k", "\[VeryThinSpace]", "g"}]], 
      SubscriptBox["\[Chi]", "g"]}]}], "=", 
