Cell[BoxData[Cell[TextData[Cell[BoxData[
  FormBox[
   RowBox[{"H", "=", 
    RowBox[{
     RowBox[{"(", GridBox[{
        {
         RowBox[{
          TemplateBox[{SubsuperscriptBox["\[CurlyPhi]", "g", "0"]},
           "Bra"], "H", 
          TemplateBox[{SubsuperscriptBox["\[CurlyPhi]", "g", "0"]},
           "Ket"]}], 
         SuperscriptBox["V", 
          RowBox[{"g", "\[VeryThinSpace]", "k"}]]},
        {
         SuperscriptBox["V", 
          RowBox[{"k", "\[VeryThinSpace]", "g"}]], 
         SuperscriptBox["H", 
          RowBox[{" ", "k"}]]}
       }], ")"}], "=", 
     RowBox[{
      RowBox[{"(", GridBox[{
         {
          RowBox[{
           SubsuperscriptBox["E", "g", "0"], "+", 
           RowBox[{
            TemplateBox[{SubsuperscriptBox["\[CurlyPhi]", "g", "0"]},
             "Bra"], "V", 
            TemplateBox[{SubsuperscriptBox["\[CurlyPhi]", "g", "0"]},
             "Ket"]}]}], 
          SuperscriptBox["V", 
           RowBox[{"g", "\[VeryThinSpace]", "k"}]]},
         {
          SuperscriptBox["V", 
           RowBox[{"k", "\[VeryThinSpace]", "g"}]], 
          SuperscriptBox["H", 
           RowBox[{" ", "k"}]]}
        }], ")"}], "=", 
      RowBox[{"(", GridBox[{
         {
          SubsuperscriptBox["E", "g", "1"], 
          SuperscriptBox["V", 
           RowBox[{"g", "\[VeryThinSpace]", "k"}]]},
         {
          SuperscriptBox["V", 
           RowBox[{"k", "\[VeryThinSpace]", "g"}]], 
          SuperscriptBox["H", 
           RowBox[{" ", "k"}]]}
        }], ")"}]}]}]}], TraditionalForm]],ExpressionUUID->
  "7b1b9a5d-33fe-44d0-a591-f4da7108e008"]],ExpressionUUID->
  "7c721ba4-a3a3-4888-8bb5-25e427de9a2a"]], "DisplayFormulaNumbered",
 CellChangeTimes->{{3.725620431520176*^9, 3.725620454617662*^9}, {
  3.7256204948921747`*^9, 3.725620621940605*^9}, {3.725620653189435*^9, 
  3.725620700486836*^9}, {3.72562194659938*^9, 3.725621965848135*^9}, {
  3.729849523970602*^9, 3.7298497030607653`*^9}, {3.729850334339246*^9, 
  3.7298503559523907`*^9}},ExpressionUUID->"ef301f40-0615-45a7-b862-\
0912d85fbfeb"],

Cell[TextData[{
 "The ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["V", 
    RowBox[{"g", "\[VeryThinSpace]", "k"}]], TraditionalForm]],
  ExpressionUUID->"52a96782-b557-4459-bd08-b7c620a2970a"],
 " matrix is ",
 Cell[BoxData[
  FormBox[
   RowBox[{"1", "\[Times]", " ", 
    RowBox[{"(", 
     RowBox[{"N", "-", "1"}], ")"}]}], TraditionalForm]],ExpressionUUID->
  "0637b237-1db4-4bd1-ad4f-cf9ec2be4d16"],
 ", a row vector, and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["V", 
     RowBox[{"k", "\[VeryThinSpace]", "g"}]], "=", 
    SuperscriptBox["V", 
     RowBox[{"g", "\[VeryThinSpace]", "k", " ", "\[Dagger]"}]]}], 
   TraditionalForm]],ExpressionUUID->"1ebea18b-c7d8-46a2-a1c5-7d3f1392b7ca"],
 " is ",
 Cell[BoxData[
  FormBox[
   RowBox[{" ", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"N", "-", "1"}], ")"}], "\[Times]", "1"}]}], TraditionalForm]],
  ExpressionUUID->"29bbcb55-0fd4-478a-8ccd-0fb1cdeac854"],
 " (column vector), and ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["H", 
    RowBox[{" ", "k", " "}]], TraditionalForm]],ExpressionUUID->
  "a3023420-ed1a-404f-80ff-63048a1fcdeb"],
 " is ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"(", 
     RowBox[{"N", "-", "1"}], ")"}], "\[Times]", 
    RowBox[{"(", 
     RowBox[{"N", "-", "1"}], ")"}]}], TraditionalForm]],ExpressionUUID->
  "48d7d0f5-9ec4-4802-a308-1905f69b2029"],
 " in size.  We shall see that to a good approximation ",
 StyleBox["we will only care about the diagonal elements ",
  FontVariations->{"Underline"->True}],
 Cell[BoxData[
  FormBox[
   SuperscriptBox["H", 
    RowBox[{"0", " ", "k", "\[VeryThinSpace]", "k"}]], TraditionalForm]],
  ExpressionUUID->"c907a46b-24aa-463d-b0ce-71b365cfa649"],
 ".\n\nWe can always write the (unnormalized) eigenvector we are after as ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    TemplateBox[{SubscriptBox["\[CurlyPhi]", "g"]},
     "Ket"], "=", 
    RowBox[{
     RowBox[{
      TemplateBox[{SubsuperscriptBox["\[CurlyPhi]", "g", "0"]},
       "Ket"], "+", 
      RowBox[{
       SubscriptBox["\[Sum]", 
        RowBox[{"k", "\[NotEqual]", "g"}]], 
       RowBox[{
        TemplateBox[{SubsuperscriptBox["\[CurlyPhi]", "k", "0"]},
         "Ket"], 
        TemplateBox[{
         SubsuperscriptBox["\[CurlyPhi]", "k", "0"],SubscriptBox["u", "g"]},
         "BraKet"]}]}]}], "=", 
     RowBox[{
      TemplateBox[{SubsuperscriptBox["\[CurlyPhi]", "g", "0"]},
       "Ket"], "+", 
      TemplateBox[{SubscriptBox["\[Chi]", "g"]},
       "Ket"]}]}]}], TraditionalForm]],ExpressionUUID->
  "00df7bfa-6aa5-4774-a0cd-f89a6a9f449d"],
 ":"
}], "Text",
 CellChangeTimes->{{3.725620731379342*^9, 3.725621019096599*^9}, {
