Cell[TextData[{
 "Our 1st order non-degenerate perturbation result for ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["m", "j"], "=", 
    RowBox[{"1", "/", "2"}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "280278c2-f397-4700-b1fe-975828f6b942"],
 " was ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["E", "j"], "=", 
    RowBox[{"\[Piecewise]", GridBox[{
       {
        RowBox[{
         RowBox[{"\[Xi]", "/", "2"}], "+", 
         RowBox[{
          FractionBox["2", "3"], 
          SubscriptBox["\[Mu]", "B"], "B"}]}], 
        RowBox[{"j", "\[Equal]", 
         RowBox[{"3", "/", "2"}]}]},
       {
        RowBox[{
         RowBox[{"-", "\[Xi]"}], "+", 
         RowBox[{
          FractionBox["1", "3"], 
          SubscriptBox["\[Mu]", "B"], "B"}]}], 
        RowBox[{"j", "\[Equal]", 
         RowBox[{"1", "/", "2"}]}]}
      }]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "4101a29d-6841-4162-9eaf-1cfdbdfa68b4"]
}], "Text",
 CellChangeTimes->{{3.729864117193727*^9, 
  3.7298642194110527`*^9}},ExpressionUUID->"6739608d-f6a4-4775-bf7a-\
233af6fee1a5"],

