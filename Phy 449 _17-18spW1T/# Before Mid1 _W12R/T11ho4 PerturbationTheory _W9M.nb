Cell[TextData[{
 "\tRewrite the matrix representation in the ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["H", "0"], "-"}], TraditionalForm]],ExpressionUUID->
  "686a94e8-d4e0-4645-b88d-f402fa73d0c3"],
 "basis as"
}], "Text",
