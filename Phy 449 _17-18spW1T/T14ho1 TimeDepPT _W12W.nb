0cb41f6a72a4"],

Cell[BoxData[{Cell[TextData[Cell[BoxData[
  FormBox[Cell[TextData[Cell[BoxData[
    FormBox[Cell[TextData[Cell[BoxData[
      FormBox[
       RowBox[{
        RowBox[{
         RowBox[{"\[ImaginaryI]", " ", "\[HBar]", " ", 
          SubscriptBox[
           OverscriptBox["a", "."], "k"]}], "+", 
         RowBox[{"\[HBar]\[Omega]", " ", 
          SubscriptBox["a", "k"]}]}], "=", 
        RowBox[{
         RowBox[{
          RowBox[{
           SubscriptBox["E", "k"], 
           SubscriptBox["a", "k"]}], "+", 
          RowBox[{
           SubscriptBox["\[ScriptCapitalV]", "kg"], 
           RowBox[{"cos", "[", 
            RowBox[{"\[Omega]", " ", "t"}], "]"}], 
           SuperscriptBox["\[ExponentialE]", 
            RowBox[{"\[ImaginaryI]", " ", "\[Omega]", " ", "t"}]], 
           SubscriptBox["a", "g"]}]}], "\[TildeTilde]", 
         RowBox[{
          RowBox[{
           SubscriptBox["E", "k"], 
