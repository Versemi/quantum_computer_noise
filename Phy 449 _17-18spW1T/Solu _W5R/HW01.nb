Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    RowBox[{"[", 
     RowBox[{
      SubscriptBox[
       OverscriptBox["p", "^"], "x"], ",", 
      SubscriptBox[
       OverscriptBox["p", "^"], "y"]}], "]"}], "\[Psi]"}], "=", 
   RowBox[{
    RowBox[{
     RowBox[{"-", "\[ImaginaryI]"}], " ", 
     RowBox[{"\[HBar]", " ", "[", 
      RowBox[{
       SubscriptBox["\[PartialD]", "x"], 
       RowBox[{",", 
        SubscriptBox["\[PartialD]", "y"]}]}], "]"}], "\[Psi]"}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"-", "\[ImaginaryI]"}], " ", 
      RowBox[{"\[HBar]", "(", 
       RowBox[{
        SubscriptBox["\[PartialD]", "x"], 
        RowBox[{
         SubscriptBox["\[PartialD]", "y"], 
         RowBox[{"-", 
          RowBox[{
           SubscriptBox["\[PartialD]", "y"], 
           SubscriptBox["\[PartialD]", "x"]}]}]}]}], ")"}], "\[Psi]"}], "=", 
     "0"}]}]}], TraditionalForm]],
 FormatType->"TraditionalForm"]], "Text",
 CellChangeTimes->{{3.693649391130497*^9, 3.6936494678485947`*^9}}]
}, Closed]],

