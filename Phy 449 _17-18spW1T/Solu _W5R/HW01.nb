Cell[CellGroupData[{

Cell[TextData[{
 "7) Why is the beam totally reflected for ",
 Cell[BoxData[
  FormBox[
   RowBox[{"Q", "<", "k", "<", 
    RowBox[{"1.4", " ", 
     RowBox[{"Q", "?"}]}]}], TraditionalForm]]]
}], "Subsubsection",
 CellChangeTimes->{{3.693651450771769*^9, 3.6936514922535887`*^9}, 
   3.693834296374511*^9, 3.693907412293491*^9}],

Cell[BoxData[{Cell[TextData[Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["\[HBar]", "2"], 
     SubsuperscriptBox["k", "i", "2"]}], "=", 
    RowBox[{
     RowBox[{
      SuperscriptBox["\[HBar]", "2"], 
      SubsuperscriptBox["k", "t", "2"]}], "+", 
     RowBox[{
      SuperscriptBox["\[HBar]", "2"], 
      SuperscriptBox["Q", "2"]}]}]}], TraditionalForm]],
  FormatType->
   "TraditionalForm"]]], "\[IndentingNewLine]", Cell[TextData[Cell[BoxData[
 Cell[TextData[Cell[BoxData[
   FormBox[
    RowBox[{
     RowBox[{
      SubsuperscriptBox["k", "ix", "2"], "+", 
      SubsuperscriptBox["k", "iz", "2"]}], "=", 
     RowBox[{
      RowBox[{
       RowBox[{
        SubsuperscriptBox["k", "tx", "2"], "+", 
        SubsuperscriptBox["k", "tz", "2"], "+", 
        SuperscriptBox["Q", "2"]}], "\[Rule]", 
       SubsuperscriptBox["k", "iz", "2"]}], "=", 
      RowBox[{
       RowBox[{
        SuperscriptBox["k", "2"], 
        SuperscriptBox[
         RowBox[{"cos", "[", 
          SubscriptBox["\[Theta]", "i"], "]"}], "2"]}], "=", 
       RowBox[{
        RowBox[{
         RowBox[{
          SubsuperscriptBox["k", "tz", "2"], "+", 
          SuperscriptBox["Q", "2"]}], "\[Rule]", 
         SubsuperscriptBox["k", "tz", "2"]}], "=", 
        RowBox[{
         RowBox[{
          SuperscriptBox["k", "2"], 
          SuperscriptBox[
           RowBox[{"cos", "[", 
            SubscriptBox["\[Theta]", "i"], "]"}], "2"]}], "-", 
         SuperscriptBox["Q", "2"], "  "}]}]}]}]}], 
    TraditionalForm]]]]]]]]]}], "Text",
 CellChangeTimes->{{3.693833762237862*^9, 3.693833766517046*^9}, {
   3.693833833824111*^9, 3.693834103444249*^9}, 3.693834296374786*^9, 
   3.69390741229396*^9}],

Cell[TextData[{
 "Therefore ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubsuperscriptBox["k", "tz", "2"], "<", "0"}], TraditionalForm]]],
 " for ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"k", "<", 
     FractionBox["Q", 
      RowBox[{"cos", "[", 
       SubscriptBox["\[Theta]", "i"], "]"}]]}], "=", 
    RowBox[{
     SqrtBox["2"], "Q", "  "}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 ". The transmitted beam is exponentially damped (evanescent) for ",
 Cell[BoxData[
  FormBox[
   RowBox[{"k", "<", 
    RowBox[{
     SqrtBox["2"], "Q"}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "."
}], "Text",
