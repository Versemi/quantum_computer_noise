Cell[TextData[{
 "for N independent atoms, define ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[DoubleStruckCapitalP]", "e"], "=", 
    RowBox[{
     FractionBox["1", "N"], 
     RowBox[{"\[Sum]", 
      SubscriptBox["P", 
       RowBox[{"e", "\[VeryThinSpace]", "i"}]]}]}]}], TraditionalForm]],
  ExpressionUUID->"6b406f3f-978f-43ae-9583-f6784d5a67a9"],
 " so ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[LeftAngleBracket]", 
     SubscriptBox["\[DoubleStruckCapitalP]", "e"], "\[RightAngleBracket]"}], 
    "=", 
    RowBox[{"\[LeftAngleBracket]", 
     SubscriptBox["P", "e"], "\[RightAngleBracket]"}]}], TraditionalForm]],
  ExpressionUUID->"b44a92a4-9e99-4487-a744-3217329008d6"],
 ".  But ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[LeftAngleBracket]", 
     SubsuperscriptBox["\[DoubleStruckCapitalP]", "e", "2"], 
     "\[RightAngleBracket]"}], "=", 
    RowBox[{
     RowBox[{
      FractionBox["1", 
       SuperscriptBox["N", "2"]], 
      RowBox[{"\[LeftAngleBracket]", 
       RowBox[{
        SubscriptBox["\[Sum]", 
         RowBox[{"i", ",", "j"}]], 
        RowBox[{
         SubscriptBox["P", 
          RowBox[{"e", "\[VeryThinSpace]", "i"}]], 
         SubscriptBox["P", 
          RowBox[{"e", "\[VeryThinSpace]", "j"}]]}]}], 
       "\[RightAngleBracket]"}]}], "=", 
     RowBox[{
      RowBox[{
       FractionBox["1", 
        SuperscriptBox["N", "2"]], 
       RowBox[{"(", 
        RowBox[{
         RowBox[{
          SubscriptBox["\[Sum]", "i"], 
          RowBox[{"\[LeftAngleBracket]", 
           SubsuperscriptBox["P", 
            RowBox[{"e", "\[VeryThinSpace]", "i"}], "2"], 
           "\[RightAngleBracket]"}]}], "+", 
         RowBox[{
          SubscriptBox["\[Sum]", 
           RowBox[{"i", "\[NotEqual]", " ", "j"}]], 
          RowBox[{
