Cell[BoxData[
 RowBox[{
  RowBox[{"sm", "=", "30"}], ";", 
  RowBox[{
   RowBox[{"\[Delta]", "[", "e_", "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"k", "=", 
       SqrtBox[
        RowBox[{"2", " ", "e"}]]}], "}"}], ",", 
     RowBox[{
      RowBox[{
       RowBox[{"pk", "[", "s_", "]"}], "=", 
       RowBox[{
        RowBox[{"NDSolve", "[", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{
            RowBox[{
             RowBox[{
              RowBox[{
               FractionBox[
                RowBox[{"-", "1"}], "2"], 
               RowBox[{
                RowBox[{"p", "''"}], "[", "s", "]"}]}], "-", 
              RowBox[{
               FractionBox[
                RowBox[{"9.5", "/", "27.2"}], 
                RowBox[{"(", 
                 RowBox[{"1", "+", 
                  SuperscriptBox[
                   RowBox[{"(", 
                    RowBox[{"s", "/", "5"}], ")"}], "6"]}], ")"}]], 
               RowBox[{"p", "[", "s", "]"}]}]}], "\[Equal]", " ", 
             RowBox[{"e", " ", 
              RowBox[{"p", "[", "s", "]"}]}]}], ",", 
            RowBox[{
             RowBox[{"p", "[", "0", "]"}], "\[Equal]", "0"}], ",", 
            RowBox[{
             RowBox[{
              RowBox[{"p", "'"}], "[", "0", "]"}], "\[Equal]", "1"}]}], "}"}],
           ",", 
          RowBox[{"p", "[", "s", "]"}], ",", 
          RowBox[{"{", 
           RowBox[{"s", ",", "0", ",", "sm"}], "}"}]}], "]"}], 
        "\[LeftDoubleBracket]", 
        RowBox[{"1", ",", "1", ",", "2"}], "\[RightDoubleBracket]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"Mod", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"ArcCot", "[", 
          FractionBox[
           RowBox[{
            RowBox[{"pk", "'"}], "[", "sm", "]"}], 
           RowBox[{"k", " ", 
            RowBox[{"pk", "[", "sm", "]"}]}]], "]"}], "-", 
         RowBox[{"k", " ", "sm"}]}], ",", "\[Pi]"}], "]"}]}]}], 
    "]"}]}]}]], "Input",
