Cell[TextData[{
 "6) Find the equation that must be satisfied by the function ",
 Cell[BoxData[
  FormBox[
   RowBox[{"G", "(", 
    RowBox[{"r", ",", 
     SuperscriptBox["r", "\[Prime]"]}], ")"}], TraditionalForm]],
  ExpressionUUID->"931deb5c-c1ed-45b4-af3b-675417c7a1f9"],
 " in order that\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["\[Psi]", "k"], "(", 
     OverscriptBox["r", "_"], ")"}], "=", 
    RowBox[{
     RowBox[{
      SubsuperscriptBox["\[Psi]", "k", "0"], "(", 
      OverscriptBox["r", "_"], ")"}], "+", 
     RowBox[{
      FractionBox[
       RowBox[{"2", "\[Mu]"}], 
       SuperscriptBox["\[HBar]", "2"]], 
      RowBox[{"\[Integral]", 
       RowBox[{
        RowBox[{
         SuperscriptBox["\[DifferentialD]", "3"], 
         SuperscriptBox[
          OverscriptBox["r", "_"], "\[Prime]"]}], 
        RowBox[{"G", "(", 
         RowBox[{
          OverscriptBox["r", "_"], ",", 
          SuperscriptBox[
           OverscriptBox["r", "_"], "\[Prime]"]}], ")"}], 
        RowBox[{"V", "(", 
         SuperscriptBox[
          OverscriptBox["r", "_"], "\[Prime]"], ")"}], 
        RowBox[{
         SubscriptBox["\[Psi]", "k"], "(", 
         SuperscriptBox[
          OverscriptBox["r", "_"], "\[Prime]"], ")"}]}]}]}]}]}], 
   TraditionalForm]],ExpressionUUID->"e3a5ba0b-a4d3-48e6-9f4b-a74704daff69"],
 "\nsolves the Schr\[ODoubleDot]dinger equation for a particle of mass ",
 Cell[BoxData[
  FormBox["\[Mu]", TraditionalForm]],ExpressionUUID->
  "68789d15-769b-48cb-b5b3-44c614d26c28"],
 " and energy ",
 Cell[BoxData[
  FormBox[
   RowBox[{"E", "=", 
    FractionBox[
     RowBox[{
      SuperscriptBox["\[HBar]", "2"], 
      SuperscriptBox["k", "2"]}], 
     RowBox[{"2", " ", "\[Mu]"}]]}], TraditionalForm]],ExpressionUUID->
  "c114e8a8-a9fe-4dcf-bf71-f83cfaab5cdf"],
 ".  ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubsuperscriptBox["\[Psi]", "k", "0"], "(", 
    OverscriptBox["r", "_"], ")"}], TraditionalForm]],ExpressionUUID->
  "4be1eeee-ea25-468c-b8c4-f2d533751f94"],
 " is a solution to the Schr\[ODoubleDot]dinger equation when ",
 Cell[BoxData[
  FormBox[
   RowBox[{"V", "=", "0"}], TraditionalForm]],ExpressionUUID->
  "4928b983-ec16-4426-a168-a0004c34d5f2"],
 "."
}], "Subsubsection",
