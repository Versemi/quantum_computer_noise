       SuperscriptBox["\[HBar]", "2"]}], 
      RowBox[{"2", " ", "M", " ", 
       SuperscriptBox["a", "2"]}]], 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"1", " ", "+", " ", 
        SubscriptBox["\[Delta]", "0"]}], ")"}], "2"]}]}]}]],
  CellChangeTimes->{{3.731934837314457*^9, 3.731934849170561*^9}, {
   3.731935154946927*^9, 3.7319351559803953`*^9}},ExpressionUUID->
  "06a8d335-58c3-4ffe-a919-d24471db2e18"],
 StyleBox["  - Ground state\n",
  FontColor->GrayLevel[0]],
 Cell[BoxData[
  RowBox[{"\t", 
   RowBox[{
    SubscriptBox["E", "00"], " ", "=", " ", 
    RowBox[{
     FractionBox[
      RowBox[{
       SuperscriptBox["\[Pi]", "2"], 
       SuperscriptBox["\[HBar]", "2"]}], 
      RowBox[{"2", " ", "M", " ", 
