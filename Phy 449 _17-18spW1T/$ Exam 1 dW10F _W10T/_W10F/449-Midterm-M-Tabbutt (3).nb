    RowBox[{"\[Lambda]", 
     RowBox[{
      UnderscriptBox["\[Sum]", 
       RowBox[{"k", "\[NotEqual]", "n"}]], 
      RowBox[{
       TemplateBox[{SuperscriptBox[
