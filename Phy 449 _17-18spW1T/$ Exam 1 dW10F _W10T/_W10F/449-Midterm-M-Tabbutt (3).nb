  RowBox[{
   SubscriptBox["\[Psi]", "total"], " ", "=", 
   RowBox[{
    RowBox[{"(", " ", 
     RowBox[{
      UnderscriptBox[
       StyleBox["\[CapitalSigma]",
        FontSize->24], 
       RowBox[{
        SubscriptBox["m", "l"], 
        SubscriptBox["m", "s"]}]], 
