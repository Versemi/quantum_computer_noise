  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{
       TemplateBox[{RowBox[{"5", "s"}]},
        "Bra"], "x", 
       TemplateBox[{RowBox[{"5", "s"}]},
