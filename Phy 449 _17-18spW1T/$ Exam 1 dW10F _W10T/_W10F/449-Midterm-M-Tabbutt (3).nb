      SubscriptBox["\[Lambda]", "x"], 
      SubscriptBox["\[Lambda]", 
       RowBox[{"5", "l"}]], " ", 
      TemplateBox[{RowBox[{"5", "s"}]},
       "Bra"], "x", 
      TemplateBox[{RowBox[{"5", "l"}]},
       "Ket"]}], " ", "=", " ", 
