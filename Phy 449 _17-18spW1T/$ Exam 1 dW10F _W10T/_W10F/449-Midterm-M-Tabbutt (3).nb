    TemplateBox[{RowBox[{"5", "s"}]},
     "Bra"], "x", 
    TemplateBox[{RowBox[{"5", "l"}]},
     "Ket"]}], "  ", "=", " ", 
   RowBox[{
    RowBox[{
     TemplateBox[{RowBox[{"5", "s"}]},
      "Bra"], 
     SuperscriptBox["\[CapitalPi]", "\[Dagger]"], "\[CapitalPi]", " ", "x", 
