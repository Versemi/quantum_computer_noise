      RowBox[{
       TemplateBox[{RowBox[{"5", "s"}]},
        "Bra"], "x", 
       TemplateBox[{RowBox[{"5", 
           SubscriptBox["p", 
            RowBox[{"ml", " ", "=", " ", 
              RowBox[{"-", "1"}]}]]}]},
        "Ket"]}], " ", "+", 
      RowBox[{
       UnderscriptBox["\[Sum]", "m"], 
       RowBox[{
        TemplateBox[{RowBox[{"5", "s"}]},
         "Bra"], "x", 
        TemplateBox[{RowBox[{"5", 
            SubscriptBox["d", 
             RowBox[{"ml", " "}]]}]},
         "Ket"]}]}], " ", "+"}], " ", "..."}], ")"}], " "}]],
