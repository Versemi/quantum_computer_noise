Cell[TextData[{
 "The second kind of Bessel function should vanish to fulfill the boundary \
condition. The solution is given by \n\t\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"P", "(", "r", ")"}], "=", 
    RowBox[{
     SubscriptBox["C", "1"], 
     SqrtBox["r"], 
     RowBox[{
      SubscriptBox["J", "q"], "(", 
      RowBox[{"k", " ", "a"}], ")"}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "0c4566c7-8048-4bd0-a43c-6e9fa9d1d73f"],
 "\n\nTo find zeros, let ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["J", "q"], "(", 
     RowBox[{"k", " ", "a"}], ")"}], "=", 
    RowBox[{
     RowBox[{"0", "\[Rule]", "k"}], "=", 
     RowBox[{
      FractionBox["1", "a"], 
      SubscriptBox["x", 
       RowBox[{"q", ",", "n"}]]}]}]}], TraditionalForm]],ExpressionUUID->
  "0b88f3fa-c5df-45c2-82e1-8f2b3cbd32de"],
 ", where ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["x", 
    RowBox[{"q", ",", "n"}]], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "712faff3-7f54-4441-9441-012b677bc231"],
 " to be the ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["n", "th"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "28d2daae-a269-40dc-90d9-b07625922cc0"],
 " zero of the ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["q", "th"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "adf87d8d-f16b-4ce2-b5f7-6861f2b91517"],
 " Bessel function; let  ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["x", 
     RowBox[{"q", ",", "n"}]], "=", 
    RowBox[{"n", " ", "\[Pi]"}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "31ea5ca2-f98e-41ab-923a-032b7fd60a32"],
 ".  When ",
 Cell[BoxData[
  FormBox[
   RowBox[{"l", "=", "0"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "96e5e9d3-6803-4dca-a780-54ffac764f4f"],
 ", it follows that  \n\t\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["E", 
     RowBox[{"n", ",", 
      RowBox[{"l", "=", "0"}]}]], "=", 
    RowBox[{
     FractionBox[
      RowBox[{
       SuperscriptBox["\[HBar]", "2"], 
       SuperscriptBox["k", "2"]}], 
      RowBox[{"2", "M"}]], "=", 
     RowBox[{
      FractionBox[
       SuperscriptBox["\[HBar]", "2"], 
       RowBox[{"2", "M"}]], 
      SuperscriptBox[
       RowBox[{"(", 
        FractionBox[
         RowBox[{"n", " ", "\[Pi]"}], "a"], ")"}], "2"]}]}]}], 
   TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "92ea259c-9920-4c7b-a4b2-b8ac098d803e"],
 " (Townsend 10.72)\n\nCompare with ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["E", 
     RowBox[{"n", ",", "l"}]], "=", 
    RowBox[{
     RowBox[{
      RowBox[{
       FractionBox[
        RowBox[{
         SuperscriptBox["\[HBar]", "2"], 
         SuperscriptBox["\[Pi]", "2"]}], 
        RowBox[{"2", "M"}]], 
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{"n", "+", 
          SubscriptBox["s", "l"]}], ")"}], "2"]}], "\[Rule]", 
      SubscriptBox["s", "0"]}], "=", "0"}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "30e18879-4f75-4517-8c58-e69d3a663096"]
}], "Text",
 CellChangeTimes->{{3.7319648811887217`*^9, 3.7319649621132765`*^9}, {
  3.7319650380629854`*^9, 3.731965067009096*^9}, {3.731965111617548*^9, 
  3.7319652531709743`*^9}, {3.731965381976854*^9, 3.7319654476086235`*^9}, {
  3.7319656161156025`*^9, 3.7319656299794464`*^9}, {3.731965696466136*^9, 
  3.731965798737878*^9}, {3.7319658397788277`*^9, 3.731965851800474*^9}, {
  3.7319676504476175`*^9, 3.7319676917233725`*^9}, {3.7319678651480665`*^9, 
  3.731967872738259*^9}},ExpressionUUID->"88f17b2a-92a5-411b-848a-\
992b88c1246e"]
}, Open  ]],

