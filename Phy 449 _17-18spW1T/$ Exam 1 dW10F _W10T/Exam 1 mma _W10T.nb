Cell[CellGroupData[{

Cell["c) -", "Subsection",
 CellChangeTimes->{{3.7319663436127653`*^9, 3.731966343947171*^9}, {
  3.731971007525876*^9, 
  3.7319710080259285`*^9}},ExpressionUUID->"213ae41a-c492-482e-a027-\
5f1f4ed02a3e"],

Cell[TextData[{
 "In an external magnetic field, \n\t\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox[
     OverscriptBox["H", "^"], "B"], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"-", 
       OverscriptBox["\[Mu]", "^"]}], "\[CenterDot]", "B"}], "=", 
     RowBox[{
      FractionBox[
       RowBox[{"|", "e", "|", "B"}], 
       RowBox[{"2", 
        SubscriptBox["m", "e"], "c"}]], " ", 
      RowBox[{"(", 
       RowBox[{
        SubscriptBox[
         OverscriptBox["J", "^"], "z"], "+", 
        SubscriptBox[
         OverscriptBox["S", "^"], "z"]}], ")"}]}]}]}], TraditionalForm]],
  ExpressionUUID->"7bc1e86f-41f3-4ee3-bec1-b4581909798e"],
 "\t\t(Townsend 11.106)\n\t\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox[
     SubscriptBox["E", "B"], 
     RowBox[{"(", "1", ")"}]], "=", 
    RowBox[{
     FractionBox[
      RowBox[{"|", "e", "|", "B"}], 
      RowBox[{"2", 
       SubscriptBox["m", "e"], "c"}]], 
     RowBox[{
      TemplateBox[{RowBox[{
          RowBox[{"j", "=", 
            RowBox[{"l", 
              UnderscriptBox["+", "_"], 
              FractionBox["1", "2"]}]}], ",", 
          SubscriptBox["m", "j"]}]},
       "Bra"], "(", 
      RowBox[{
       SubscriptBox[
        OverscriptBox["J", "^"], "z"], "+", 
       SubscriptBox[
        OverscriptBox["S", "^"], "z"]}], ")"}], 
     TemplateBox[{RowBox[{
         RowBox[{"j", "=", 
           RowBox[{"l", 
             UnderscriptBox["+", "_"], 
             FractionBox["1", "2"]}]}], ",", 
         SubscriptBox["m", "j"]}]},
      "Ket"]}]}], TraditionalForm]],ExpressionUUID->
  "d43d2f25-412c-43b4-9d29-aac2843d9bf2"],
 "\t\t(T 11.107)\n\t\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[LeftAngleBracket]", 
     SubscriptBox["S", "z"], "\[RightAngleBracket]"}], "=", 
    RowBox[{
     UnderscriptBox["+", "_"], 
     FractionBox[
      RowBox[{
       SubscriptBox["m", "j"], "\[HBar]"}], 
      RowBox[{
       RowBox[{"2", "l"}], "+", "1"}]]}]}], TraditionalForm]],ExpressionUUID->
  "9571e5b5-776d-414c-b831-8f1aae3980cc"],
 "\t\t(T 11.108)\n\t\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[LeftAngleBracket]", 
     SubscriptBox["J", "z"], "\[RightAngleBracket]"}], "=", 
    RowBox[{
     SubscriptBox["m", "j"], "\[HBar]"}]}], TraditionalForm]],ExpressionUUID->
  "7ba139aa-c762-4718-9de1-04074e0a44fa"],
 "\t\t(T 3.25b)\n\nGiven that ",
 Cell[BoxData[
  FormBox[
   RowBox[{"s", "=", "0"}], TraditionalForm]],ExpressionUUID->
  "5bd55038-f0d4-4b45-8267-d61ac3fa1b53"],
 ", with ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"|", 
      RowBox[{"s", "-", "l"}], "|", 
      RowBox[{"\[LessEqual]", "j", "\[LessEqual]", 
       RowBox[{"s", "+", "l"}]}]}], "\[Rule]", "j"}], "=", "l"}], 
   TraditionalForm]],ExpressionUUID->"52d0a3c8-a414-46e6-8fcc-9e288aeb1eb0"],
 ", then \n\t\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[LeftAngleBracket]", 
     SubscriptBox["S", "z"], "\[RightAngleBracket]"}], "=", 
    RowBox[{
     RowBox[{
      UnderscriptBox["+", "_"], 
      FractionBox[
       RowBox[{
        SubscriptBox["m", "j"], "\[HBar]"}], 
       RowBox[{
        RowBox[{"2", "j"}], "+", "1"}]]}], "=", "0"}]}], TraditionalForm]],
  ExpressionUUID->"424a12c7-8da6-44f1-84e2-2e83696b9ad0"],
 "\n\nThe first order correction is given by \n\t\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     TemplateBox[{RowBox[{"j", ",", 
         SubscriptBox["m", "j"]}]},
      "Bra"], 
     SubscriptBox[
      OverscriptBox["H", "^"], "B"], 
     TemplateBox[{RowBox[{"j", ",", 
         SubscriptBox["m", "j"]}]},
      "Ket"]}], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["m", "j"], "\[HBar]", " ", 
      FractionBox[
       RowBox[{"|", "e", "|", "B"}], 
       RowBox[{"2", 
        SubscriptBox["m", "e"], "c"}]]}], "=", 
     RowBox[{
      SubscriptBox["m", "l"], "\[HBar]", 
      FractionBox[
       RowBox[{"|", "e", "|", "B"}], 
       RowBox[{"2", 
        SubscriptBox["m", "e"], "c"}]]}]}]}], TraditionalForm]],
  ExpressionUUID->"f1b54fd7-e5b5-4978-bdf3-f3c8b4541168"]
}], "Text",
 CellChangeTimes->{{3.731968149665516*^9, 3.7319682277176085`*^9}, {
  3.731968328705138*^9, 3.7319683367047424`*^9}, {3.7319702472288523`*^9, 
  3.73197060325601*^9}, {3.731970686665197*^9, 3.731970713294664*^9}, {
  3.7319707545554276`*^9, 3.731970917379361*^9}, {3.731970964560848*^9, 
  3.731970980749863*^9}, {3.731971371332415*^9, 3.7319714112570066`*^9}, {
  3.731971453601466*^9, 3.73197150583403*^9}, {3.731982474937329*^9, 
  3.7319825357739577`*^9}},ExpressionUUID->"e5bf095a-3c9e-4230-bc93-\
5d17b3c6ccb8"]
}, Open  ]],

Cell[CellGroupData[{

Cell["d)", "Subsection",
 CellChangeTimes->{{3.7319710178999634`*^9, 
  3.731971018134361*^9}},ExpressionUUID->"60016836-2a6c-46ca-9bdd-\
f83da4e30f86"],

Cell[TextData[{
 "Given three spin-0 particles, two of them can occupy the same state. The \
energy of three particles in the first excited states separately is given by \
\n\t\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["E", "10"], " ", "=", " ", 
     RowBox[{
      FractionBox[
       RowBox[{
        SuperscriptBox["\[Pi]", "2"], 
        SuperscriptBox["\[HBar]", "2"]}], 
       RowBox[{"2", " ", "M", " ", 
        SuperscriptBox["a", "2"]}]], 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"1", " ", "+", " ", 
         SubscriptBox["\[Delta]", "0"]}], ")"}], "2"]}]}], ";", 
    RowBox[{
     SubscriptBox["E", "10"], " ", "=", " ", 
     RowBox[{
      FractionBox[
       RowBox[{
        SuperscriptBox["\[Pi]", "2"], 
        SuperscriptBox["\[HBar]", "2"]}], 
       RowBox[{"2", " ", "M", " ", 
        SuperscriptBox["a", "2"]}]], 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"1", " ", "+", " ", 
         SubscriptBox["\[Delta]", "0"]}], ")"}], "2"]}]}], ";", 
    RowBox[{
     SubscriptBox["E", "11"], " ", "=", " ", 
     RowBox[{
      FractionBox[
       RowBox[{
        SuperscriptBox["\[Pi]", "2"], 
        SuperscriptBox["\[HBar]", "2"]}], 
       RowBox[{"2", " ", "M", " ", 
        SuperscriptBox["a", "2"]}]], 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"1", " ", "+", " ", 
         SubscriptBox["\[Delta]", "1"]}], ")"}], "2"]}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "c2a853b6-b601-4e4a-bfad-e38a0143e700"],
 "\nThe total energy in the first excited state is given by \n\t\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["E", "1"], "=", 
    RowBox[{
     FractionBox[
      RowBox[{
       SuperscriptBox["\[Pi]", "2"], 
       SuperscriptBox["\[HBar]", "2"]}], 
      RowBox[{"2", " ", "M", " ", 
       SuperscriptBox["a", "2"]}]], "[", 
     RowBox[{
      RowBox[{"2", 
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{"1", " ", "+", " ", 
          SubscriptBox["\[Delta]", "0"]}], ")"}], "2"]}], "+", 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"1", " ", "+", " ", 
         SubscriptBox["\[Delta]", "1"]}], ")"}], "2"]}], "]"}]}], 
   TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "195b7ffc-0f4c-4cae-898e-4a1e28b68418"],
 "\n\t\t\nThe states of the particles can be arranged as \n\t\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    TemplateBox[{RowBox[{
        RowBox[{"1", "s"}], ";", 
        RowBox[{"1", "s"}], ";", 
        RowBox[{"1", "p"}]}]},
     "Ket"], ",", 
    TemplateBox[{RowBox[{
        RowBox[{"1", "s"}], ";", 
        RowBox[{"1", "p"}], ";", 
        RowBox[{"1", "s"}]}]},
     "Ket"], ",", 
    TemplateBox[{RowBox[{
        RowBox[{"1", "p"}], ";", 
        RowBox[{"1", "s"}], ";", 
        RowBox[{"1", "s"}], ";"}]},
     "Ket"]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "67b679ee-4bed-4901-82b6-2b7674c3159b"],
 "\nHence, the ",
 Cell[BoxData[
  FormBox[
   RowBox[{"degeneracy", "=", "3"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "12e90bdd-0d00-4f89-b1ab-4980e5c6a41e"],
 ".\n\t\t"
}], "Text",
 CellChangeTimes->{{3.7319827693281546`*^9, 3.731983153523212*^9}, 
   3.7319831976904144`*^9},ExpressionUUID->"fb46d22e-23b2-47b8-8e03-\
6e63cb709ccb"]
}, Open  ]]
}, Open  ]],

