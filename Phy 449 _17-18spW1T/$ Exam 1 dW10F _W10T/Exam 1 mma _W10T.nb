da82433c4ed3"],

Cell[TextData[{
 "Given the same value of ",
 Cell[BoxData[
  FormBox["n", TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "5c260b58-60c6-4c02-80b5-5d9189b3b8e1"],
 ", the larger ",
 Cell[BoxData[
  FormBox["l", TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "cef44d05-fcfd-49b9-85b9-c9b5cb9e19f5"],
 " corresponds to the larger energy. Given that ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"l", "=", "0"}], ",", "1", ",", "2"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "4a3eb551-ca74-4151-8872-3853bfbe3c7f"],
 ", it follows that \n\t\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["E", 
     RowBox[{"n", ",", "0"}]], "<", 
    SubscriptBox["E", 
     RowBox[{"n", ",", "1"}]], "<", 
    SubscriptBox["E", 
     RowBox[{"n", ",", "2"}]]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "ae7556f4-1865-4639-bdfc-b596e1c74096"],
 "\nGiven that ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["E", 
     RowBox[{"n", ",", "l"}]], "=", 
    RowBox[{
     FractionBox[
      RowBox[{
       SuperscriptBox["\[HBar]", "2"], 
       SuperscriptBox["\[Pi]", "2"]}], 
      RowBox[{"2", "M"}]], 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"n", "+", 
        SubscriptBox["s", "l"]}], ")"}], "2"]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "a859783c-641b-42c2-ae6b-6ad017fe009f"],
 ", then\n\t\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["s", "0"], "<", 
    SubscriptBox["s", "1"], "<", 
    SubscriptBox["s", "2"]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "43c69203-467d-4890-8407-dedb7785a6a3"]
}], "Text",
 CellChangeTimes->{{3.7319662060282526`*^9, 3.731966325826942*^9}, {
  3.7319679089617996`*^9, 
  3.7319679263079677`*^9}},ExpressionUUID->"a63c4f3e-6162-409a-a7c7-\
f123dce1109c"]
