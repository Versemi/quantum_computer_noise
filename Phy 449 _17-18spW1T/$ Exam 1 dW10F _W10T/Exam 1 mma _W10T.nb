       RowBox[{
        RowBox[{
         FractionBox["1", "2"], " ", 
         RowBox[{"(", 
          RowBox[{"1", "+", 
           RowBox[{"2", " ", "l"}]}], ")"}]}], ",", 
        RowBox[{
         SqrtBox["2"], " ", "r", " ", 
         SqrtBox["\[Epsilon]"]}]}], "]"}], " ", 
      RowBox[{"C", "[", "1", "]"}]}], "+", 
     RowBox[{
      SqrtBox["r"], " ", 
      RowBox[{"BesselY", "[", 
       RowBox[{
        RowBox[{
         FractionBox["1", "2"], " ", 
         RowBox[{"(", 
          RowBox[{"1", "+", 
           RowBox[{"2", " ", "l"}]}], ")"}]}], ",", 
        RowBox[{
         SqrtBox["2"], " ", "r", " ", 
         SqrtBox["\[Epsilon]"]}]}], "]"}], " ", 
      RowBox[{"C", "[", "2", "]"}]}]}]}], "}"}], "}"}]], "Output",
