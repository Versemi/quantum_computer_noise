       SubscriptBox["r", "12"]]}], "=", 
     RowBox[{
      RowBox[{
       SubscriptBox[
        OverscriptBox["H", "^"], "0"], "+", 
       SubscriptBox["V", "12"]}], "=", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         FractionBox[
          RowBox[{"-", "1"}], "2"], 
         RowBox[{
          SuperscriptBox[
           SubscriptBox["\[Del]", "1"], "2"], 
          RowBox[{"-", 
           FractionBox["Z", 
            SubscriptBox["r", "1"]]}]}]}], ")"}], "+", 
       RowBox[{"(", 
        RowBox[{
         FractionBox[
          RowBox[{"-", "1"}], "2"], 
         RowBox[{
          SuperscriptBox[
           SubscriptBox["\[Del]", "2"], "2"], 
          RowBox[{"-", 
           FractionBox["Z", 
            SubscriptBox["r", "2"]]}]}]}], ")"}], "+", 
       FractionBox["1", 
        SubscriptBox["r", "12"]]}]}]}]}], TraditionalForm]],ExpressionUUID->
  "f67025e9-7884-45b8-b63f-2d0af9ab81c0"],
