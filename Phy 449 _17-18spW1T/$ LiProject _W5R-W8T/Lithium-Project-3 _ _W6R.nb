Cell[CellGroupData[{

Cell["Slater Determinants and the Pauli Exclusion Principle", "Subsection",
 CellChangeTimes->{{3.728769733859709*^9, 3.728769745203074*^9}},
 Background->RGBColor[
  0.88, 1, 0.88],ExpressionUUID->"d6fbc52b-b04e-4194-8c4f-d4bf7fc1617c"],

Cell[TextData[{
 StyleBox["8) Show that a Slater determinant for Li can be written as  ",
  FontColor->RGBColor[0, 0, 1]],
 Cell[BoxData[
  FormBox[
   RowBox[{
    TemplateBox[{RowBox[{
        RowBox[{"\[DoubleVerticalBar]", "ad"}], ",", "bu", ",", 
        RowBox[{"cu", "\[DoubleVerticalBar]"}]}]},
     "Ket"], "=", 
    RowBox[{
     RowBox[{
      FractionBox["1", 
       SqrtBox["6"]], 
      RowBox[{"(", 
       RowBox[{"1", "+", "\[ScriptCapitalL]", "+", "\[ScriptCapitalR]"}], 
       ")"}], 
      RowBox[{"(", 
       RowBox[{"1", "-", 
        SubscriptBox["P", "23"]}], ")"}], 
      TemplateBox[{RowBox[{"ad", ",", "bu", ",", "cu"}]},
       "Ket"]}], "=", 
     RowBox[{
      FractionBox["1", 
       SqrtBox["6"]], "\[ScriptCapitalA]", 
      TemplateBox[{RowBox[{"ad", ",", "bu", ",", "cu"}]},
       "Ket"]}]}]}], TraditionalForm]],ExpressionUUID->
  "f7a2d586-a397-49e5-93c4-bf9ffe812d37"],
 StyleBox[". We might call the operator ",
  FontColor->RGBColor[0, 0, 1]],
 "\[ScriptCapitalA] ",
 StyleBox["an \[OpenCurlyDoubleQuote]",
  FontColor->RGBColor[0, 0, 1]],
 "antisymmetrizer",
 StyleBox["\[CloseCurlyDoubleQuote] as it converts a product state into a \
Slater determinant.",
  FontColor->RGBColor[0, 0, 1]]
}], "Text",
 CellChangeTimes->{{3.728771440924498*^9, 3.7287714430750313`*^9}, {
  3.728772451433517*^9, 3.728772453654235*^9}, {3.728847892734272*^9, 
  3.728847966942737*^9}},ExpressionUUID->"e32e5c44-aa25-4844-adb8-\
8c48dec93a6b"],

Cell[TextData[Cell[BoxData[{
 FormBox[
  RowBox[{
   TemplateBox[{RowBox[{
       RowBox[{"\[DoubleVerticalBar]", 
         RowBox[{"a", " ", "d"}]}], ",", 
       RowBox[{"b", " ", "u"}], ",", 
       RowBox[{
         RowBox[{"c", " ", "u"}], "\[DoubleVerticalBar]"}]}]},
    "Ket"], "=", 
   RowBox[{"det", 
    RowBox[{"\[LeftBracketingBar]", GridBox[{
       {
        RowBox[{
         RowBox[{"\[CurlyPhi]", "[", "a", "]"}], " ", 
         RowBox[{"d", "(", "1", ")"}]}], 
        RowBox[{
         RowBox[{"\[CurlyPhi]", "[", "b", "]"}], " ", 
         RowBox[{"u", "(", "1", ")"}]}], 
        RowBox[{
         RowBox[{"\[CurlyPhi]", "[", "c", "]"}], " ", "u", 
         RowBox[{"(", "1", ")"}]}]},
       {
        RowBox[{
         RowBox[{"\[CurlyPhi]", "[", "a", "]"}], " ", 
         RowBox[{"d", "(", "2", ")"}]}], 
        RowBox[{
         RowBox[{"\[CurlyPhi]", "[", "b", "]"}], " ", 
