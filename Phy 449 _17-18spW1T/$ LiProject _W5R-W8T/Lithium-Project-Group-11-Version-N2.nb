  FormBox[
   RowBox[{
    TemplateBox[{RowBox[{
        SubscriptBox["r", "1"], 
        SubscriptBox["r", "2"], 
        SubscriptBox["r", "3"]}],RowBox[{
        RowBox[{"1", "s"}], ";", 
        RowBox[{"1", "s"}], ";", 
        RowBox[{"2", "s"}]}]},
     "BraKet"], "=", 
    RowBox[{
     RowBox[{
      SubsuperscriptBox["P", "1", "z"], "(", 
      SubscriptBox["r", "1"], ")"}], 
     RowBox[{
      SubsuperscriptBox["P", "1", "z"], "(", 
      SubscriptBox["r", "2"], ")"}], 
     RowBox[{
      SubsuperscriptBox["P", "2", "z"], "(", 
      SubscriptBox["r", "3"], ")"}]}]}], TraditionalForm]],ExpressionUUID->
  "0918f4d2-5065-408b-960f-07a632fae133"],
 " with again ",
 Cell[BoxData[
  FormBox["z", TraditionalForm]],ExpressionUUID->
  "e6ceb58d-d9d8-4abc-98a6-610ad3148a93"],
 " being an adjustable parameter."
}], "Text",
