 "\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{
            RowBox[{
             RowBox[{
              RowBox[{
               RowBox[{
                RowBox[{
                 RowBox[{
                  RowBox[{
                   RowBox[{
                    RowBox[{
                    RowBox[{"\[ScriptCapitalL]", "^", "3"}], " ", "|", 
                    RowBox[{"a", " ", "b", " ", "c"}]}], 
                    "\[RightAngleBracket]"}], " ", "=", " ", 
                   RowBox[{
                    RowBox[{
                    "\[ScriptCapitalL]", " ", "\[ScriptCapitalL]", " ", 
                    "\[ScriptCapitalL]"}], " ", "|", 
                    RowBox[{"a", " ", "b", " ", "c"}]}]}], 
                  "\[RightAngleBracket]"}], " ", "=", " ", 
                 RowBox[{
                  RowBox[{
                  "P13", " ", "P23", " ", "P13", " ", "P23", " ", "P13", " ", 
                   "P23"}], " ", "|", 
                  RowBox[{"a", " ", "b", " ", "c"}]}]}], 
                "\[RightAngleBracket]"}], " ", "=", " ", 
               RowBox[{
                RowBox[{
                "P13", " ", "P23", " ", "P13", " ", "P23", " ", "P13"}], " ", 
                "|", 
                RowBox[{"a", " ", "c", " ", "b"}]}]}], 
              "\[RightAngleBracket]"}], " ", "=", " ", 
             RowBox[{
              RowBox[{"P13", " ", "P23", " ", "P13", " ", "P23"}], "  ", "|", 
              
              RowBox[{"b", " ", "c", " ", "a"}]}]}], "\[RightAngleBracket]"}],
            "  ", "=", " ", 
           RowBox[{
            RowBox[{"P13", " ", "P23", " ", "P13"}], "   ", "|", 
            RowBox[{"b", " ", "a", " ", "c"}]}]}], "\[RightAngleBracket]"}], 
         "   ", "\n", 
         RowBox[{"=", " ", 
          RowBox[{
           RowBox[{"P13", " ", "P23"}], "  ", "|", 
           RowBox[{"c", " ", "a", " ", "b"}]}]}]}], "\[RightAngleBracket]"}], 
       "  ", "=", " ", 
       RowBox[{"P13", "  ", "|", 
        RowBox[{"c", " ", "b", " ", "a"}]}]}], "\[RightAngleBracket]"}], "  ",
      "=", "   ", 
     RowBox[{"|", 
      RowBox[{"a", " ", "b", " ", "c"}]}]}], "\[RightAngleBracket]"}], 
   TraditionalForm]],ExpressionUUID->"26e6fbbb-c447-4b36-9943-e3cc32867240"],
 "    Thus ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["\[ScriptCapitalL]", "3"], TraditionalForm]],
  ExpressionUUID->"bdbe7fba-a7e9-43b8-97e1-f5fc34eb99fb"],
 StyleBox["= 1 ",
  FontWeight->"Bold"],
 ", where 1 is the identity matrix. \n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{
       SuperscriptBox["\[ScriptCapitalL]", "\[Dagger]"], " ", "|", 
       RowBox[{"a", " ", "b", " ", "c"}]}], "\[RightAngleBracket]"}], "=", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{"P13", 
        SuperscriptBox[
         RowBox[{"P23", ")"}], "\[Dagger]"]}], " ", "|", 
       RowBox[{"a", " ", "b", " ", "c"}]}]}]}], "\[RightAngleBracket]"}], 
   TraditionalForm]],ExpressionUUID->"0f906c3d-83cd-41b8-b720-92313dbee351"],
 "  \t\t( A B ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox[")", "\[Dagger]"], " ", "=", " ", 
    RowBox[{
     SuperscriptBox["B", "\[Dagger]"], " ", 
