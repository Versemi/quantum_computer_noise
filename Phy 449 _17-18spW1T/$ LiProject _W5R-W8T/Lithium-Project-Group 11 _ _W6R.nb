  FormatType->"TraditionalForm",ExpressionUUID->
  "1fb0446b-28d9-41d8-a892-388f42577b85"],
 "\t\t\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"[", 
      RowBox[{
       RowBox[{
        FractionBox[
         RowBox[{"-", "1"}], "2"], 
        FractionBox[
         SuperscriptBox["d", "2"], 
         SuperscriptBox["ds", "2"]]}], "+", " ", 
       FractionBox[
        RowBox[{"l", 
         RowBox[{"(", 
          RowBox[{"l", "+", "1"}], ")"}]}], 
        RowBox[{"2", " ", 
         SuperscriptBox["s", "2"], " "}]], "-", 
       FractionBox[
        RowBox[{"m", " ", 
         SuperscriptBox["\[Alpha]", "2"], 
         SuperscriptBox["e", "2"], "Z", " "}], 
        RowBox[{
         SuperscriptBox["\[HBar]", "2"], "\[Alpha]", " ", "s"}]]}], "]"}], 
     SubscriptBox["P", 
      RowBox[{"n", " ", "l"}]]}], " ", "=", " ", 
    RowBox[{
     SubscriptBox["E", "s"], " ", 
     SubscriptBox["P", 
      RowBox[{"n", " ", "l"}]]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "87f58db1-ea5c-4947-86ed-7a01588c662a"],
 "\n\nFind \[Alpha] by making potential as form above: ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox[
     RowBox[{"m", " ", 
      SuperscriptBox["\[Alpha]", "2"], 
      SuperscriptBox["e", "2"], "Z", " "}], 
     RowBox[{
      SuperscriptBox["\[HBar]", "2"], "\[Alpha]", " ", "s"}]], " ", "=", 
    RowBox[{
     RowBox[{
      FractionBox[
       RowBox[{"1", " "}], 
       RowBox[{" ", "s"}]], "\[Rule]", 
      FractionBox[
       RowBox[{"m", " ", 
        SuperscriptBox["\[Alpha]", "2"], 
        SuperscriptBox["e", "2"], "Z", " "}], 
       RowBox[{
        SuperscriptBox["\[HBar]", "2"], "\[Alpha]", " "}]]}], "=", 
     RowBox[{
      RowBox[{"1", "\[Rule]", "\[Alpha]"}], "=", 
      FractionBox[
       RowBox[{" ", 
        SuperscriptBox["\[HBar]", "2"]}], 
       RowBox[{"Z", " ", "m", " ", 
        SuperscriptBox["e", "2"]}]]}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "fae9be21-ee4e-45d2-9197-3193c58d378f"],
 "\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["a", "o"], "=", 
    RowBox[{
     FractionBox[
      RowBox[{" ", 
       SuperscriptBox["\[HBar]", "2"]}], 
      RowBox[{"m", " ", 
       SuperscriptBox["e", "2"]}]], " ", "=", 
     RowBox[{
      RowBox[{
       RowBox[{"The", " ", "Bohr", " ", "Radius"}], "\[Rule]", "  ", 
       "\[Alpha]"}], " ", "=", " ", 
      RowBox[{
       RowBox[{
        FractionBox[
         SubscriptBox["a", "o"], "Z"], "\[Rule]", "  ", "r"}], " ", "=", " ", 
       
       RowBox[{
        FractionBox[
         SubscriptBox["a", "o"], "Z"], 
        StyleBox[" ",
         FontWeight->"Bold"], 
        StyleBox["s",
         FontWeight->"Bold"]}]}]}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
