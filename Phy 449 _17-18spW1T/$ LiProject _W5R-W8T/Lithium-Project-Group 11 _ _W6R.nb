Cell[TextData[{
 StyleBox["\tFind the normalization factor using scaling instead: ",
  FontSize->18,
  FontWeight->"Bold"],
 "\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"E\[Psi]", " ", "=", " ", 
    RowBox[{"H", " ", "\[Psi]"}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "7dcf2c8a-68ec-4d57-b7c7-7b02c565976a"],
 "\t\t\n\nTake the Radial part: ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"[", 
      RowBox[{
       RowBox[{
        FractionBox[
         RowBox[{"-", 
          SuperscriptBox["\[HBar]", "2"]}], 
         RowBox[{"2", "m"}]], 
        FractionBox[
         SuperscriptBox["d", "2"], 
         SuperscriptBox["dr", "2"]]}], "+", " ", 
       FractionBox[
        RowBox[{"l", 
         RowBox[{"(", 
          RowBox[{"l", "+", "1"}], ")"}], 
         SuperscriptBox["\[HBar]", "2"]}], 
        RowBox[{"2", "m", " ", 
         SuperscriptBox["r", "2"]}]], "-", 
       FractionBox[
        RowBox[{
         SuperscriptBox["e", "2"], "Z"}], "r"]}], "]"}], 
     SubscriptBox["P", 
      RowBox[{"n", " ", "l"}]]}], " ", "=", " ", 
    RowBox[{"E", " ", 
     SubscriptBox["P", 
      RowBox[{"n", " ", "l"}]]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "3f4ada7a-c707-4c62-910c-154743dfcd6a"],
 "\t(Townsend 9.98)\n\nRescale: ",
 Cell[BoxData[
  FormBox[
   RowBox[{"r", "=", 
    RowBox[{"\[Alpha]", " ", "s"}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "49342d03-3650-4afd-b2ac-3ebf4881dbd3"],
 "\t\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"[", 
      RowBox[{
       RowBox[{
        FractionBox[
         RowBox[{"-", 
          SuperscriptBox["\[HBar]", "2"]}], 
         RowBox[{"2", "m", " ", 
          SuperscriptBox["\[Alpha]", "2"]}]], 
        FractionBox[
         SuperscriptBox["d", "2"], 
         SuperscriptBox["ds", "2"]]}], "+", " ", 
       FractionBox[
        RowBox[{"l", 
         RowBox[{"(", 
          RowBox[{"l", "+", "1"}], ")"}], 
         SuperscriptBox["\[HBar]", "2"]}], 
        RowBox[{"2", " ", "m", " ", 
         SuperscriptBox["s", "2"], " ", 
         SuperscriptBox["\[Alpha]", "2"]}]], "-", 
       FractionBox[
        RowBox[{
         SuperscriptBox["e", "2"], "Z"}], 
        RowBox[{"\[Alpha]", " ", "s"}]]}], "]"}], 
     SubscriptBox["P", 
      RowBox[{"n", " ", "l"}]]}], " ", "=", " ", 
    RowBox[{"E", " ", 
     SubscriptBox["P", 
      RowBox[{"n", " ", "l"}]]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "ed704595-211b-4a0f-8f4e-40801860edd7"],
 "\n\n\t\t\t\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      FractionBox[
       RowBox[{"m", " ", 
        SuperscriptBox["\[Alpha]", "2"]}], 
       SuperscriptBox["\[HBar]", "2"]], "[", 
      RowBox[{
       RowBox[{
        FractionBox[
         RowBox[{"-", 
          SuperscriptBox["\[HBar]", "2"]}], 
         RowBox[{"2", "m", " ", 
          SuperscriptBox["\[Alpha]", "2"]}]], 
        FractionBox[
         SuperscriptBox["d", "2"], 
         SuperscriptBox["ds", "2"]]}], "+", " ", 
       FractionBox[
        RowBox[{"l", 
         RowBox[{"(", 
          RowBox[{"l", "+", "1"}], ")"}], 
         SuperscriptBox["\[HBar]", "2"]}], 
        RowBox[{"2", " ", "m", " ", 
         SuperscriptBox["s", "2"], " ", 
         SuperscriptBox["\[Alpha]", "2"]}]], "-", 
       FractionBox[
        RowBox[{
         SuperscriptBox["e", "2"], "Z"}], 
        RowBox[{"\[Alpha]", " ", "s"}]]}], "]"}], 
     SubscriptBox["P", 
      RowBox[{"n", " ", "l"}]]}], " ", "=", 
    RowBox[{
     FractionBox[
      RowBox[{"m", " ", 
       SuperscriptBox["\[Alpha]", "2"]}], 
      SuperscriptBox["\[HBar]", "2"]], " ", "E", " ", 
     SubscriptBox["P", 
      RowBox[{"n", " ", "l"}]]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "1ec5fbf7-0228-4dbc-a660-e49fe95b300b"],
 "\n\t\t\t\t\t\t\n\t\t\t\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"[", 
      RowBox[{
       RowBox[{
        FractionBox[
         RowBox[{"-", "1"}], "2"], 
        FractionBox[
         SuperscriptBox["d", "2"], 
         SuperscriptBox["ds", "2"]]}], "+", " ", 
       FractionBox[
        RowBox[{"l", 
         RowBox[{"(", 
          RowBox[{"l", "+", "1"}], ")"}]}], 
        RowBox[{"2", " ", 
         SuperscriptBox["s", "2"], " "}]], "-", 
       FractionBox[
        RowBox[{"m", " ", 
         SuperscriptBox["\[Alpha]", "2"], 
         SuperscriptBox["e", "2"], "Z", " "}], 
        RowBox[{
         SuperscriptBox["\[HBar]", "2"], "\[Alpha]", " ", "s"}]]}], "]"}], 
     SubscriptBox["P", 
      RowBox[{"n", " ", "l"}]]}], " ", "=", 
    RowBox[{
     FractionBox[
      RowBox[{"m", " ", 
       SuperscriptBox["\[Alpha]", "2"]}], 
      SuperscriptBox["\[HBar]", "2"]], " ", "E", " ", 
     SubscriptBox["P", 
      RowBox[{"n", " ", "l"}]]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "913ee169-2dc3-45f4-9b66-dcad9a747f62"],
 "\t\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["E", "s"], " ", "=", " ", 
