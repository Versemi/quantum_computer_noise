Cell[TextData[{
 StyleBox["\nShow that ",
  FontWeight->"Bold"],
 Cell[BoxData[
  FormBox[
   SuperscriptBox["\[ScriptCapitalL]", "3"], TraditionalForm]],
  FontWeight->"Bold",ExpressionUUID->"fd8540fc-117b-4ff4-87db-7c8cca68aff8"],
 StyleBox["= 1 ",
  FontWeight->"Bold"],
 Cell[BoxData[
  FormBox[
   RowBox[{"and", " ", 
    SuperscriptBox["\[ScriptCapitalL]", "\[Dagger]"]}], TraditionalForm]],
  FontWeight->"Bold",ExpressionUUID->"6a0f8d68-c12b-4366-8ec9-19f1f8e3260b"],
 StyleBox[" = \[ScriptCapitalR] .\n\n",
  FontWeight->"Bold"],
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["\[ScriptCapitalL]", "3"], " ", "=", " ", 
    RowBox[{
     RowBox[{
     "\[ScriptCapitalL]", " ", "\[ScriptCapitalL]", " ", 
      "\[ScriptCapitalL]"}], " ", "=", " ", 
     RowBox[{
     "P13", " ", "P23", " ", "P13", " ", "P23", " ", "P13", " ", "P23"}]}]}], 
   TraditionalForm]],ExpressionUUID->"0d329b7e-5e25-4bf7-a46b-8ecedfe78198"],
 "\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{
            RowBox[{
             RowBox[{
              RowBox[{
               RowBox[{
                RowBox[{
                 RowBox[{
                  RowBox[{
                   RowBox[{
                    RowBox[{
                    RowBox[{"\[ScriptCapitalL]", "^", "3"}], " ", "|", 
                    RowBox[{"a", " ", "b", " ", "c"}]}], 
                    "\[RightAngleBracket]"}], " ", "=", " ", 
                   RowBox[{
                    RowBox[{
                    "\[ScriptCapitalL]", " ", "\[ScriptCapitalL]", " ", 
                    "\[ScriptCapitalL]"}], " ", "|", 
                    RowBox[{"a", " ", "b", " ", "c"}]}]}], 
                  "\[RightAngleBracket]"}], " ", "=", " ", 
                 RowBox[{
                  RowBox[{
                  "P13", " ", "P23", " ", "P13", " ", "P23", " ", "P13", " ", 
                   "P23"}], " ", "|", 
                  RowBox[{"a", " ", "b", " ", "c"}]}]}], 
                "\[RightAngleBracket]"}], " ", "=", " ", 
               RowBox[{
                RowBox[{
                "P13", " ", "P23", " ", "P13", " ", "P23", " ", "P13"}], " ", 
                "|", 
                RowBox[{"a", " ", "c", " ", "b"}]}]}], 
              "\[RightAngleBracket]"}], " ", "=", " ", 
             RowBox[{
              RowBox[{"P13", " ", "P23", " ", "P13", " ", "P23"}], "  ", "|", 
              
              RowBox[{"b", " ", "c", " ", "a"}]}]}], "\[RightAngleBracket]"}],
            "  ", "=", " ", 
           RowBox[{
            RowBox[{"P13", " ", "P23", " ", "P13"}], "   ", "|", 
            RowBox[{"b", " ", "a", " ", "c"}]}]}], "\[RightAngleBracket]"}], 
         "   ", "\n", 
         RowBox[{"=", " ", 
          RowBox[{
           RowBox[{"P13", " ", "P23"}], "  ", "|", 
           RowBox[{"c", " ", "a", " ", "b"}]}]}]}], "\[RightAngleBracket]"}], 
       "  ", "=", " ", 
       RowBox[{"P13", "  ", "|", 
        RowBox[{"c", " ", "b", " ", "a"}]}]}], "\[RightAngleBracket]"}], "  ",
      "=", "   ", 
     RowBox[{"|", 
      RowBox[{"a", " ", "b", " ", "c"}]}]}], "\[RightAngleBracket]"}], 
   TraditionalForm]],ExpressionUUID->"26e6fbbb-c447-4b36-9943-e3cc32867240"],
 "    Thus ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["\[ScriptCapitalL]", "3"], TraditionalForm]],
  ExpressionUUID->"bdbe7fba-a7e9-43b8-97e1-f5fc34eb99fb"],
 StyleBox["= 1 ",
  FontWeight->"Bold"],
 ", where 1 is the identity matrix. \n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{
       SuperscriptBox["\[ScriptCapitalL]", "\[Dagger]"], " ", "|", 
       RowBox[{"a", " ", "b", " ", "c"}]}], "\[RightAngleBracket]"}], "=", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{"P13", 
        SuperscriptBox[
         RowBox[{"P23", ")"}], "\[Dagger]"]}], " ", "|", 
       RowBox[{"a", " ", "b", " ", "c"}]}]}]}], "\[RightAngleBracket]"}], 
   TraditionalForm]],ExpressionUUID->"0f906c3d-83cd-41b8-b720-92313dbee351"],
 "  \t\t( A B ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox[")", "\[Dagger]"], " ", "=", " ", 
    RowBox[{
     SuperscriptBox["B", "\[Dagger]"], " ", 
     SuperscriptBox["A", "\[Dagger]"]}]}], TraditionalForm]],ExpressionUUID->
  "8d5e2135-3688-4027-a76a-cc1fa191c4b7"],
 "\n\nFor arbitrary operator and  state: ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{"M", 
        TemplateBox[{"k"},
         "Ket"]}], "=", 
       RowBox[{"m", "|", "k"}]}], "\[RightAngleBracket]"}], 
     TemplateBox[{"k"},
      "Bra"], " ", 
     FormBox[
      SuperscriptBox["M", "\[Dagger]"],
      TraditionalForm]}], " ", "=", 
    RowBox[{"m", 
     TemplateBox[{"k"},
      "Bra"], " "}]}], TraditionalForm]],ExpressionUUID->
  "a7599716-e2cd-4933-9719-f40ad3a72c86"],
 "\n\nFor \[ScriptCapitalL]  Op: \t",
 Cell[BoxData[
  FormBox[
   RowBox[{" ", 
    RowBox[{
     RowBox[{
      TemplateBox[{RowBox[{"a", " ", "b", " ", "c"}]},
       "Bra"], 
      FormBox[
       RowBox[{"\[ScriptCapitalL]", " "}],
       TraditionalForm], 
      TemplateBox[{RowBox[{"d", " ", "e", " ", "f"}]},
       "Ket"]}], " ", "=", 
     TemplateBox[{
      RowBox[{"a", " ", "b", " ", "c"}],
       RowBox[{"e", " ", "f", " ", "d", " "}]},
      "BraKet"]}]}], TraditionalForm]],ExpressionUUID->
  "2fa2e12c-ad4a-4c48-b899-b6a9488caf7a"],
 " \t\t\nFor ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["\[ScriptCapitalL]", "\[Dagger]"], TraditionalForm]],
  ExpressionUUID->"8db34b69-7b9c-43d9-b632-9e1d1923affa"],
 "  Op: \t ",
 Cell[BoxData[
  FormBox[
   RowBox[{" ", 
    RowBox[{
     RowBox[{
      TemplateBox[{RowBox[{"a", " ", "b", " ", "c"}]},
       "Bra"], 
      FormBox[
       RowBox[{
        SuperscriptBox["\[ScriptCapitalL]", "\[Dagger]"], " "}],
       TraditionalForm], 
      TemplateBox[{RowBox[{"d", " ", "e", " ", "f"}]},
       "Ket"]}], " ", "=", 
     RowBox[{
      RowBox[{
       TemplateBox[{RowBox[{"d", " ", "e", " ", "f"}]},
        "Bra"], 
       FormBox[
        RowBox[{"\[ScriptCapitalL]", " "}],
        TraditionalForm], 
       TemplateBox[{RowBox[{"a", " ", "b", " ", "c"}]},
        "Ket"]}], " ", "=", 
      TemplateBox[{
       RowBox[{"d", " ", "e", " ", "f"}],
        RowBox[{"b", " ", "c", " ", "a", " "}]},
       "BraKet"]}]}]}], TraditionalForm]],ExpressionUUID->
  "72b8de30-03f2-418c-beb7-e9523993d044"],
 "\n\nFor  ",
 StyleBox["\[ScriptCapitalR]",
  FontWeight->"Bold"],
 " Op: \t ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     TemplateBox[{RowBox[{"a", " ", "b", " ", "c"}]},
      "Bra"], 
     FormBox[
      RowBox[{
       StyleBox["\[ScriptCapitalR]",
        FontWeight->"Bold"], " "}],
      TraditionalForm], 
     TemplateBox[{RowBox[{"d", " ", "e", " ", "f"}]},
      "Ket"]}], " ", "=", 
    TemplateBox[{
     RowBox[{"a", " ", "b", " ", "c"}],RowBox[{"f", " ", "d", " ", "e", " "}]},
     "BraKet"], " "}], TraditionalForm]],ExpressionUUID->
  "4fa8da92-fc24-4838-8bf6-df9cbadeb85c"],
 "\t\t\nFor ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["\[ScriptCapitalR]", "\[Dagger]"], TraditionalForm]],
  ExpressionUUID->"8a9d288c-6b6e-483f-9b3a-9615adbca044"],
 "  Op:  \t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     TemplateBox[{RowBox[{"a", " ", "b", " ", "c"}]},
      "Bra"], 
     FormBox[
      RowBox[{
       SuperscriptBox[
        StyleBox["\[ScriptCapitalR]",
         FontWeight->"Bold"], "\[Dagger]"], " "}],
      TraditionalForm], 
     TemplateBox[{RowBox[{"d", " ", "e", " ", "f"}]},
      "Ket"]}], " ", "=", 
    RowBox[{
     RowBox[{
      TemplateBox[{RowBox[{"d", " ", "e", " ", "f"}]},
       "Bra"], 
      FormBox[
       RowBox[{
        StyleBox["\[ScriptCapitalR]",
         FontWeight->"Bold"], " "}],
       TraditionalForm], 
      TemplateBox[{RowBox[{"a", " ", "b", " ", "c"}]},
       "Ket"]}], " ", "=", 
     TemplateBox[{
      RowBox[{"d", " ", "e", " ", "f"}],
       RowBox[{"c", " ", "a", " ", "b", " "}]},
      "BraKet"], " "}]}], TraditionalForm]],ExpressionUUID->
  "805f5382-7c83-459f-a38a-0ca9ff35e0d3"],
 "\t\n\nBut you can notice:\t \t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["\[ScriptCapitalL]", "\[Dagger]"], " ", "=", 
    RowBox[{
     TemplateBox[{
      RowBox[{"d", " ", "e", " ", "f"}],
       RowBox[{"b", " ", "c", " ", "a", " "}]},
      "BraKet"], "  ", "=", 
     RowBox[{
      RowBox[{
       TemplateBox[{"d","b"},
        "BraKet"], 
       TemplateBox[{"e","c"},
        "BraKet"], 
       TemplateBox[{"f","a"},
        "BraKet"]}], " ", "=", 
      RowBox[{
       RowBox[{
        TemplateBox[{"a","f"},
         "BraKet"], "  ", 
        TemplateBox[{"b","d"},
         "BraKet"], 
        RowBox[{"\[LeftAngleBracket]", 
         RowBox[{"c", "\[VerticalSeparator]", "e"}], 
         "\[RightAngleBracket]"}]}], "=", 
       RowBox[{
        TemplateBox[{
         RowBox[{"a", " ", "b", " ", "c"}],
          RowBox[{"f", " ", "d", " ", "e", " "}]},
         "BraKet"], " ", "=", "\[ScriptCapitalR]"}]}]}]}]}], 
   TraditionalForm]],ExpressionUUID->"aaded9a5-018a-41aa-bba2-daa138c702d7"],
 StyleBox["\n",
  FontWeight->"Bold"]
}], "Text",
