     RowBox[{
      RowBox[{
       RowBox[{"0", "*", "P23"}], " ", "+", " ", 
       RowBox[{"P13", " ", "*", "0"}]}], "   ", "=", " ", "0"}]}]}], 
   TraditionalForm]],ExpressionUUID->"56b7d15e-57e7-467f-aa50-f821ead1665e"],
