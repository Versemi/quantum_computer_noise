        RowBox[{"Factorial", "[", 
         RowBox[{"n", "+", "l"}], "]"}], "*", "2", "n"}]], 
      SuperscriptBox[
       RowBox[{"(", 
        FractionBox[
         RowBox[{"2", "Z"}], 
         RowBox[{"n", "*", "a"}]], ")"}], "3"]}], ")"}], 
    RowBox[{"1", "/", "2"}]], 
   SuperscriptBox[
