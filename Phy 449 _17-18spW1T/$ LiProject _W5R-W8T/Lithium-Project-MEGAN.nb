Cell[TextData[{
 "The Hamiltonian for the two-electron ",
 Cell[BoxData[
  SuperscriptBox["Li", "+"]],
  CellChangeTimes->{{3.7278740894113283`*^9, 3.7278740923072157`*^9}},
  ExpressionUUID->"b405b210-6fba-46c9-a4be-a0c72bfd6cab"],
 " ion is:\n\n",
 Cell[BoxData[
  RowBox[{
   SuperscriptBox[
    OverscriptBox["H", "^"], 
    RowBox[{"+", " "}]], "=", " ", 
   RowBox[{
    RowBox[{
     RowBox[{
      SuperscriptBox[
       OverscriptBox["H", "^"], "++"], 
      RowBox[{"(", "1", ")"}]}], " ", "+", "  ", 
     RowBox[{
      SuperscriptBox[
       OverscriptBox["H", "^"], "++"], 
      RowBox[{"(", "2", ")"}]}], " ", "+", " ", 
     FractionBox["1", 
      SubscriptBox["r", "12"]]}], " ", "=", " ", 
    RowBox[{
     RowBox[{
      SubscriptBox[
       OverscriptBox["H", "^"], "o"], " ", "+", " ", 
      SubscriptBox["V", "12"]}], " ", "=", " ", 
     RowBox[{
      RowBox[{
       FractionBox[
        RowBox[{"-", "1"}], "2"], 
       RowBox[{
        SuperscriptBox[
         SubscriptBox["\[Del]", "1"], "2"], 
        RowBox[{"-", 
         FractionBox["Z", "r"]}]}]}], " ", "-", " ", 
      RowBox[{
       FractionBox["1", 
        RowBox[{"2", " "}]], 
       RowBox[{
        SuperscriptBox[
         SubscriptBox["\[Del]", "2"], "2"], " ", 
        RowBox[{"-", " ", 
         FractionBox["Z", "r"]}]}]}], "  ", "+", " ", 
