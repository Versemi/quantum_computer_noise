 RowBox[{
  RowBox[{"RpsiRpsiH", "[", 
   RowBox[{"r_", ",", "n_", ",", "l_"}], "]"}], ":=", " ", 
  RowBox[{
   SuperscriptBox[
    RowBox[{"(", 
     RowBox[{
      FractionBox[
       RowBox[{"Factorial", "[", 
        RowBox[{"(", 
         RowBox[{"n", "-", "l", "-", "1"}], ")"}], "]"}], 
       RowBox[{
        RowBox[{"Factorial", "[", 
         RowBox[{"n", "+", "l"}], "]"}], "*", "2", "n"}]], 
