Cell[TextData[{
 StyleBox["Find the normalization factor using scaling instead: ",
  FontSize->18,
  FontWeight->"Bold"],
 "\n\n",
 Cell[BoxData[
  RowBox[{"E\[Psi]", " ", "=", " ", 
   RowBox[{"H", " ", "\[Psi]"}]}]],
  CellChangeTimes->{{3.7278740894113283`*^9, 3.7278741819061403`*^9}, 
