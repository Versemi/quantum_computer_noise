  ExpressionUUID->"bd43574e-3afa-4cba-8a4a-3b3f34a6c974"],
 "\t\t\n\nTake the Radial part: \t\t",
 Cell[BoxData[
  RowBox[{
   RowBox[{
    RowBox[{"[", 
     RowBox[{
      RowBox[{
       FractionBox[
        RowBox[{"-", 
         SuperscriptBox["\[HBar]", "2"]}], 
        RowBox[{"2", "*", "m"}]], 
       FractionBox[
        SuperscriptBox["d", "2"], 
        SuperscriptBox["dr", "2"]]}], "+", " ", 
      FractionBox[
       RowBox[{"l", 
        RowBox[{"(", 
         RowBox[{"l", "+", "1"}], ")"}], 
        SuperscriptBox["\[HBar]", "2"]}], 
       RowBox[{"2", "*", "m", "*", 
        SuperscriptBox["r", "2"]}]], "-", 
      FractionBox[
       RowBox[{
        SuperscriptBox["e", "2"], "Z"}], "r"]}], "]"}], 
    SubscriptBox["P", "nl"]}], " ", "=", " ", 
   RowBox[{"E", " ", 
    SubscriptBox["P", "nl"]}]}]],
  CellChangeTimes->{{3.7285880611820087`*^9, 3.728588209680243*^9}},
  ExpressionUUID->"cda59846-eea4-4321-95df-38000d060640"],
 "     \t\t\t(Townsend 9.98)\n\nRescale:    r = \[Alpha] s\t\t\t",
 Cell[BoxData[
  RowBox[{
   RowBox[{
    RowBox[{"[", 
     RowBox[{
      RowBox[{
       FractionBox[
        RowBox[{"-", 
         SuperscriptBox["\[HBar]", "2"]}], 
        RowBox[{"2", "m", " ", 
         SuperscriptBox["\[Alpha]", "2"]}]], 
       FractionBox[
        SuperscriptBox["d", "2"], 
        SuperscriptBox["ds", "2"]]}], "+", " ", 
      FractionBox[
       RowBox[{"l", 
        RowBox[{"(", 
         RowBox[{"l", "+", "1"}], ")"}], 
        SuperscriptBox["\[HBar]", "2"]}], 
       RowBox[{"2", " ", "m", " ", 
        SuperscriptBox["s", "2"], " ", 
        SuperscriptBox["\[Alpha]", "2"]}]], "-", 
      FractionBox[
       RowBox[{
        SuperscriptBox["e", "2"], "Z"}], 
       RowBox[{"\[Alpha]", " ", "s"}]]}], "]"}], 
    SubscriptBox["P", "nl"]}], " ", "=", " ", 
   RowBox[{"E", " ", 
    SubscriptBox["P", "nl"]}]}]],
  CellChangeTimes->{{3.7285880611820087`*^9, 3.728588209680243*^9}, {
   3.728588400110444*^9, 3.7285884295103493`*^9}},ExpressionUUID->
  "722c8f8a-aae7-4678-8eef-9c389e7642e9"],
 "\n\n\t\t\t\t\t\t",
 Cell[BoxData[
  RowBox[{
   RowBox[{
    RowBox[{
     FractionBox[
      RowBox[{"m", " ", 
       SuperscriptBox["\[Alpha]", "2"]}], 
      SuperscriptBox["\[HBar]", "2"]], "[", 
     RowBox[{
      RowBox[{
       FractionBox[
        RowBox[{"-", 
         SuperscriptBox["\[HBar]", "2"]}], 
        RowBox[{"2", "m", " ", 
         SuperscriptBox["\[Alpha]", "2"]}]], 
       FractionBox[
        SuperscriptBox["d", "2"], 
        SuperscriptBox["ds", "2"]]}], "+", " ", 
      FractionBox[
       RowBox[{"l", 
        RowBox[{"(", 
         RowBox[{"l", "+", "1"}], ")"}], 
        SuperscriptBox["\[HBar]", "2"]}], 
       RowBox[{"2", " ", "m", " ", 
        SuperscriptBox["s", "2"], " ", 
        SuperscriptBox["\[Alpha]", "2"]}]], "-", 
      FractionBox[
       RowBox[{
        SuperscriptBox["e", "2"], "Z"}], 
       RowBox[{"\[Alpha]", " ", "s"}]]}], "]"}], 
    SubscriptBox["P", "nl"]}], " ", "=", 
   RowBox[{
    FractionBox[
     RowBox[{"m", " ", 
      SuperscriptBox["\[Alpha]", "2"]}], 
     SuperscriptBox["\[HBar]", "2"]], " ", "E", " ", 
    SubscriptBox["P", "nl"]}]}]],
