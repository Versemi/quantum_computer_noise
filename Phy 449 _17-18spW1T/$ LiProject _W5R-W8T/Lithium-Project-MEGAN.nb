         RowBox[{"n", "*", "a"}]], ")"}], "3"]}], ")"}], 
    RowBox[{"1", "/", "2"}]], 
   SuperscriptBox[
    RowBox[{"(", 
     RowBox[{"2", 
      FractionBox["r", 
       RowBox[{"n", "*", "a"}]]}], ")"}], "l"], 
   RowBox[{"LaguerreL", "[", 
    RowBox[{
     RowBox[{"n", "-", "l", "-", "1"}], ",", 
