Cell["\<\
This is a totally antisymmetric wavefunction, called a Slater determinant.  \
The name comes from the property that it can be calculated by\
\>", "Text",
 CellChangeTimes->{{3.72515173735156*^9, 3.725151772662511*^9}, {
  3.7251518189992647`*^9, 
  3.725151845529097*^9}},ExpressionUUID->"b4359238-bb62-44f0-9046-\
79565256c992"],

Cell[BoxData[Cell[TextData[Cell[BoxData[Cell[TextData[Cell[BoxData[
   FormBox[Cell[TextData[Cell[BoxData[
     FormBox[
      RowBox[{
       TemplateBox[{RowBox[{"\[LeftDoubleBracketingBar]", 
           RowBox[{
             RowBox[{"a", " ", "d"}], ",", 
             RowBox[{"b", " ", "u"}], ",", 
             RowBox[{"c", " ", "u"}]}], "\[RightDoubleBracketingBar]"}]},
        "Ket"], "=", 
       RowBox[{"det", "\[VerticalSeparator]", GridBox[{
          {
           RowBox[{
            RowBox[{"\[CurlyPhi]", "[", "a", "]"}], " ", "d", " ", 
            RowBox[{"(", "1", ")"}]}], 
           RowBox[{
            RowBox[{"\[CurlyPhi]", "[", "b", "]"}], "u", " ", 
            RowBox[{"(", "1", ")"}]}], 
           RowBox[{
            RowBox[{"\[CurlyPhi]", "[", "c", "]"}], "\[VeryThinSpace] ", "u", 
            " ", 
            RowBox[{"(", "1", ")"}]}]},
          {
           RowBox[{
            RowBox[{"\[CurlyPhi]", "[", "a", "]"}], " ", 
            RowBox[{"d", "(", "2", ")"}]}], 
           RowBox[{
            RowBox[{"\[CurlyPhi]", "[", "b", "]"}], "u", " ", 
            RowBox[{"(", "2", ")"}]}], 
           RowBox[{
            RowBox[{"\[CurlyPhi]", "[", "c", "]"}], "\[VeryThinSpace] ", 
            RowBox[{"u", "(", "2", ")"}]}]},
          {
           RowBox[{
            RowBox[{"\[CurlyPhi]", "[", "a", "]"}], " ", 
            RowBox[{"d", "(", "3", ")"}]}], 
           RowBox[{
            RowBox[{"\[CurlyPhi]", "[", "b", "]"}], "u", " ", 
            RowBox[{"(", "3", ")"}]}], 
           RowBox[{
            RowBox[{"\[CurlyPhi]", "[", "c", "]"}], "\[VeryThinSpace] ", 
            RowBox[{"u", "(", "3", ")"}]}]}
         }], "\[VerticalSeparator]"}]}], TraditionalForm]],ExpressionUUID->
     "a7bfd391-f1af-4060-be65-96dc30aa040b"]],ExpressionUUID->
     "50fa6227-0276-43f3-a4dc-5321ca6b83e0"], TraditionalForm]],
   ExpressionUUID->"01d0f6f9-5c37-4f5b-b4d1-b859a96ddc79"]],ExpressionUUID->
