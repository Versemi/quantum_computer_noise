    RowBox[{"{", 
     RowBox[{"f", "=", 
      RowBox[{
       SuperscriptBox[
        RowBox[{"(", "r", ")"}], "n"], 
       SuperscriptBox["\[ExponentialE]", 
        FractionBox[
         RowBox[{"-", "r"}], "n"]]}]}], "}"}], ",", 
    RowBox[{
     RowBox[{"Do", "[", 
      RowBox[{
       RowBox[{"f", "=", 
        RowBox[{
         SubscriptBox["A\[Dagger]", "lp"], "@", "f"}]}], ",", 
       RowBox[{"{", 
        RowBox[{"lp", ",", 
         RowBox[{"n", "-", "2"}], ",", "l", ",", 
         RowBox[{"-", "1"}]}], "}"}]}], "]"}], ";", 
     RowBox[{"f", "//", "Simplify"}], ";", 
     RowBox[{
      FractionBox["f", 
       SuperscriptBox[
        RowBox[{"Integrate", "[", 
         RowBox[{
          SuperscriptBox["f", "2"], ",", 
          RowBox[{"{", 
           RowBox[{"r", ",", "0", ",", "\[Infinity]"}], "}"}]}], "]"}], 
        RowBox[{"1", "/", "2"}]]], "//", "Simplify"}]}]}], "]"}]}]}], "Input",
