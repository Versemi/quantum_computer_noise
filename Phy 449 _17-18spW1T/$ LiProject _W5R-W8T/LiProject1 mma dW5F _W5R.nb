 RowBox[{
  RowBox[{
   SubscriptBox["A\[Dagger]", "l_"], "@", "f_"}], ":=", 
  RowBox[{
   RowBox[{"-", 
    RowBox[{
     SubscriptBox["\[PartialD]", "r"], "f"}]}], "+", 
   RowBox[{
    RowBox[{"(", 
