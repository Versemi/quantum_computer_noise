Cell[TextData[{
 "There are many cavity modes ",
 Cell[BoxData[
  FormBox[
   RowBox[{"l", " "}], TraditionalForm]],ExpressionUUID->
  "ba1f50b0-e4fd-4baa-887d-4d215e9b5e22"],
 "whose frequency is close to ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[Omega]", "eg"], TraditionalForm]],ExpressionUUID->
  "eca3843e-cc01-4e8a-8342-3f2cd9fbfb5c"],
 ".  The modes, which we take to have the electric field ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox["E", "^"], "=", 
    RowBox[{
     SubscriptBox["\[ScriptCapitalE]", "l"], "  ", 
     SuperscriptBox["\[ExponentialE]", 
      RowBox[{"\[ImaginaryI]", " ", 
       StyleBox[
        RowBox[{"k", "\[CenterDot]", "r"}],
        FontWeight->"Bold"]}]], " ", 
     RowBox[{"(", 
      RowBox[{"a", "+", 
       SuperscriptBox["a", "\[Dagger]"]}], ")"}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "83ef5b5a-6d50-4b85-8de2-c7c2c9af28b0"],
 " over our box with  have wavevectors ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox[
     StyleBox["k",
      FontWeight->"Bold"], "l"], 
    StyleBox["=",
     FontWeight->"Bold"], 
    RowBox[{
     RowBox[{
      FractionBox[
       StyleBox[
        RowBox[{"2", "\[Pi]"}],
        FontWeight->"Bold"], "L"], 
      RowBox[{"(", 
       RowBox[{
        SubscriptBox["m", "x"], ",", 
        SubscriptBox["m", "y"], ",", 
        SubscriptBox["m", "z"]}], ")"}]}], "=", 
     RowBox[{
      FractionBox[
       RowBox[{"2", "\[Pi]", " ", "m"}], "L"], " ", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"sin\[Theta]", " ", "cos\[Phi]"}], ",", 
        RowBox[{"sin\[Theta]", " ", "sin\[Phi]"}], ",", " ", "cos\[Theta]"}], 
       ")"}]}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "2f976fdb-490b-4632-b1a8-a403c91d6253"],
 ", and two polarizations ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox[
     StyleBox["\[Epsilon]",
      FontWeight->"Bold"], "1"], "=", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{"cos\[Theta]", " ", "cos\[Phi]"}], ",", 
      RowBox[{"cos\[Theta]", " ", "sin\[Phi]"}], ",", " ", 
      RowBox[{"-", "sin\[Theta]"}]}], ")"}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "c3711cdd-0f94-4770-9925-fa7d9bc7e92e"],
 " and  ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox[
     StyleBox["\[Epsilon]",
      FontWeight->"Bold"], "2"], "=", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{"-", "sin\[Phi]"}], ",", "cos\[Phi]", ",", "0"}], ")"}]}], 
   TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "80fb0c68-e255-4d03-9953-e2ec5f6abbbf"],
 ".     We can therefore write, using ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["\[Omega]", "l"], "=", 
     RowBox[{
      RowBox[{"c", " ", 
       SubscriptBox["k", "l"]}], "=", 
      RowBox[{"2", "\[Pi]", " ", "m", " ", 
       FractionBox["c", "L"]}]}]}], ","}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "ec410995-2d17-42fb-bb6a-0a95113fa42b"]
}], "Text",
