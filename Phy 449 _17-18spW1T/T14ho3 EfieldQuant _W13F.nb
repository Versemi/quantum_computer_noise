  FormBox[
   RowBox[{
    RowBox[{"H", 
     TemplateBox[{"n"},
      "Ket"]}], "=", 
    RowBox[{
     RowBox[{"\[HBar]\[Omega]", "(", 
      RowBox[{"n", "+", 
       FractionBox["1", "2"]}], ")"}], 
