Cell[BoxData[Cell[TextData[Cell[BoxData[
  FormBox[Cell[TextData[Cell[BoxData[Cell[TextData[Cell[BoxData[
     FormBox[Cell[TextData[Cell[BoxData[
       FormBox[
        RowBox[{"\[CapitalGamma]", "=", 
         RowBox[{
          RowBox[{
           SubscriptBox["\[Sum]", "\[Epsilon]"], 
           RowBox[{"\[Integral]", 
            RowBox[{
             RowBox[{"\[DifferentialD]", 
              SubscriptBox["m", "x"]}], 
             RowBox[{"\[DifferentialD]", 
              SubscriptBox["m", "y"]}], 
             RowBox[{"\[DifferentialD]", 
              SubscriptBox["m", "z"]}], 
             FractionBox[
              RowBox[{"\[Gamma]", "  ", 
               SubsuperscriptBox["\[CapitalOmega]", "l", "2"]}], 
              RowBox[{
               SuperscriptBox["\[Gamma]", "2"], "+", 
               RowBox[{"4", " ", 
                SuperscriptBox[
                 RowBox[{"(", 
                  RowBox[{
                   SubscriptBox["\[Omega]", "eg"], "-", 
                   SubscriptBox["\[Omega]", "l"]}], ")"}], "2"]}]}]]}]}]}], 
          "=", 
          RowBox[{
           RowBox[{
            SubscriptBox["\[Sum]", "\[Epsilon]"], 
            RowBox[{
             SuperscriptBox[
              RowBox[{"(", 
               RowBox[{
                FractionBox[
                 RowBox[{"2", " ", "e"}], "\[HBar]"], " ", 
                SqrtBox[
                 FractionBox[
                  RowBox[{"2", "\[Pi]", " ", "\[HBar]", " "}], "V"]]}], ")"}],
               "2"], 
             RowBox[{"\[Integral]", 
              RowBox[{
               SuperscriptBox["m", "2"], 
               RowBox[{"\[DifferentialD]", "m"}], " ", "sin\[Theta]", " ", 
               RowBox[{"\[DifferentialD]", "\[Theta]"}], 
               RowBox[{"\[DifferentialD]", "\[Phi]"}], " ", 
               FractionBox[
                RowBox[{
                 SubscriptBox["\[Omega]", "l"], 
                 SuperscriptBox[
                  RowBox[{"\[LeftBracketingBar]", 
                   RowBox[{
                    TemplateBox[{"e"},
                    "Bra"], 
                    RowBox[{
                    StyleBox["r",
                    FontWeight->"Bold"], 
                    StyleBox["\[CenterDot]",
                    FontWeight->"Bold"], 
                    SubscriptBox[
                    StyleBox["\[Epsilon]",
                    FontWeight->"Bold"], "l"]}], 
                    TemplateBox[{"g"},
                    "Ket"]}], "\[RightBracketingBar]"}], "2"], "\[Gamma]"}], 
                RowBox[{
