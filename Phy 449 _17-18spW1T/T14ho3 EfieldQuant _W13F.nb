Cell[TextData[{
 "We want to write the Hamiltonian in terms of the electric field instead of \
the vector potential.  Let\[CloseCurlyQuote]s make a unitary transformation \
of the Hamiltonian, with ",
 Cell[BoxData[
  FormBox[
   RowBox[{"U", "=", 
    SuperscriptBox["\[ExponentialE]", 
     RowBox[{
      RowBox[{"-", "\[ImaginaryI]"}], " ", "x", " ", 
      FractionBox[
       RowBox[{"e", " ", 
        RowBox[{
         SubscriptBox["A", "x"], "(", "t", ")"}]}], 
       RowBox[{"\[HBar]", " ", "c"}]]}]]}], TraditionalForm]],ExpressionUUID->
  "fd2d9d24-557c-4225-90a0-b5b3d6268d88"],
 ".  Now, assuming we can ignore gradients of ",
 Cell[BoxData[
  FormBox["A", TraditionalForm]],ExpressionUUID->
  "e47b2562-bcb0-4625-b8a5-3d42a2e48871"],
 ", ",
 Cell[BoxData[
  FormBox[Cell[TextData[Cell[BoxData[
    FormBox[Cell[TextData[Cell[BoxData[
      FormBox[
       RowBox[{
        RowBox[{
         RowBox[{"(", 
          RowBox[{
           StyleBox["p",
            FontWeight->"Bold"], "+", 
           FractionBox[
            RowBox[{"e", " ", 
             StyleBox["A",
              FontWeight->"Bold"]}], "c"]}], ")"}], " ", "U"}], "=", 
        RowBox[{
         RowBox[{
          RowBox[{"U", "(", 
           RowBox[{
            StyleBox["p",
             FontWeight->"Bold"], "+", 
            FractionBox[
             RowBox[{"e", " ", 
              StyleBox["A",
               FontWeight->"Bold"]}], "c"]}], ")"}], "-", 
          RowBox[{"\[ImaginaryI]", " ", "\[HBar]", " ", 
           RowBox[{"(", 
            RowBox[{
             StyleBox["\[Del]",
              FontWeight->"Bold"], 
             StyleBox["U",
              FontWeight->"Plain"]}], 
            StyleBox[")",
             FontWeight->"Plain"]}]}]}], 
         StyleBox["=",
          FontWeight->"Plain"], 
         RowBox[{
          RowBox[{
           RowBox[{"U", "(", 
            RowBox[{
             StyleBox["p",
              FontWeight->"Bold"], "+", 
             FractionBox[
              RowBox[{"e", " ", 
               StyleBox["A",
                FontWeight->"Bold"]}], "c"]}], ")"}], "-", 
           RowBox[{"\[ImaginaryI]", " ", "\[HBar]", " ", "U", " ", 
            RowBox[{"(", 
             RowBox[{
              RowBox[{"-", "\[ImaginaryI]"}], "  ", 
              FractionBox[
               RowBox[{"e", " ", 
                SubscriptBox["A", "x"]}], 
               RowBox[{"\[HBar]", " ", "c"}]]}], ")"}]}]}], "=", 
          RowBox[{"U", " ", 
           StyleBox["p",
            FontWeight->"Bold"]}]}]}]}], TraditionalForm]],ExpressionUUID->
      "5da6d9ef-7d96-4bd6-a8d3-a8a9c736d7c8"]],ExpressionUUID->
      "48f50ad5-befa-4875-9f5a-0c876dbe7660"], TraditionalForm]],
    ExpressionUUID->"44d21dcd-2df6-41b3-8935-92c4e8a5828b"]],ExpressionUUID->
    "79dd7fcb-6cb3-4746-a31f-f97aaf6b8c0f"], TraditionalForm]],
  ExpressionUUID->"f8b5f8d3-1d69-4636-a819-3c1b560fb82c"],
 "\nand\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[ImaginaryI]", " ", "\[HBar]", 
     RowBox[{
      SubscriptBox["\[PartialD]", "t"], "U"}], " ", "\[Psi]"}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"U", " ", "\[ImaginaryI]", " ", "\[HBar]", 
       RowBox[{
        SubscriptBox["\[PartialD]", "t"], "\[Psi]"}]}], "+", 
      RowBox[{"\[ImaginaryI]", " ", "\[HBar]", " ", "U", " ", "\[Psi]", " ", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"-", "\[ImaginaryI]"}], " ", "x", " ", 
         FractionBox[
          RowBox[{"e", " "}], 
          RowBox[{"\[HBar]", " ", "c"}]], 
         RowBox[{
          SubscriptBox["\[PartialD]", "t"], 
          SubscriptBox["A", "x"]}]}], ")"}]}]}], "=", 
     RowBox[{
      RowBox[{"\[ImaginaryI]", " ", "\[HBar]", " ", "U", " ", 
       RowBox[{
        SubscriptBox["\[PartialD]", "t"], "\[Psi]"}]}], "+", 
      RowBox[{"U", " ", 
       RowBox[{"(", " ", 
        RowBox[{
         RowBox[{"-", " ", "e"}], " ", "x", " ", 
         SubscriptBox["E", "x"]}], ")"}], "\[Psi]", " "}]}]}]}], 
   TraditionalForm]],ExpressionUUID->"684a9f21-c9d8-4656-b36e-210e6f94bae7"]
