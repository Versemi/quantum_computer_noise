Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    RowBox[{"\[ImaginaryI]", " ", "\[HBar]", " ", 
     RowBox[{
      SubscriptBox["\[PartialD]", "t"], "\[Psi]"}]}], " ", "-", " ", 
    RowBox[{"e", " ", "x", " ", 
     SubscriptBox["E", "x"], " ", "\[Psi]"}]}], "=", 
   RowBox[{
    RowBox[{
     RowBox[{
      SuperscriptBox["U", "\[Dagger]"], "H", " ", "U", " ", "\[Psi]"}], "+", 
     SubscriptBox["H", "field"]}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{
       FractionBox["1", 
        RowBox[{"2", " ", "m"}]], 
       SuperscriptBox[
        RowBox[{
         SuperscriptBox["U", "\[Dagger]"], "(", 
