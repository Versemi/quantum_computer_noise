     RowBox[{
      RowBox[{
       SqrtBox[
        FractionBox[
         RowBox[{"4", "\[Pi]", " ", "\[HBar]", " ", "\[Omega]"}], "V"]], 
       RowBox[{"(", 
        RowBox[{"a", "+", 
         SuperscriptBox["a", "\[Dagger]"]}], ")"}]}], "=", 
      RowBox[{
       SubscriptBox["\[ScriptCapitalE]", "1"], "(", 
