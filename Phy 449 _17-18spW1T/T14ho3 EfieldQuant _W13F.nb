Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"[", 
    RowBox[{
     OverscriptBox["N", "^"], ",", "\[ScriptCapitalE]"}], "]"}], "=", 
   RowBox[{
    RowBox[{
     SubscriptBox["\[ScriptCapitalE]", "1"], "[", 
     RowBox[{"N", ",", 
      RowBox[{"(", 
       RowBox[{"a", "+", 
        SuperscriptBox["a", "\[Dagger]"]}], ")"}]}], "]"}], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["\[ScriptCapitalE]", "1"], "(", 
      RowBox[{
       SuperscriptBox["a", "\[Dagger]"], "-", "a"}], ")"}], "=", 
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{"-", "\[ImaginaryI]"}], " ", "\[ScriptCapitalB]"}], "\[Rule]",
        " ", 
       RowBox[{
        RowBox[{
        "\[CapitalDelta]\[ScriptCapitalE]", " ", "\[CapitalDelta]", 
         "\[VeryThinSpace]", "N"}], "\[GreaterEqual]", " ", 
        RowBox[{
         FractionBox["1", "2"], 
         RowBox[{"\[LeftBracketingBar]", 
          RowBox[{"[", 
           RowBox[{"\[ScriptCapitalE]", ",", "N"}], "]"}], 
          "\[RightBracketingBar]"}]}]}]}], "=", 
      RowBox[{
       FractionBox["1", "2"], 
       RowBox[{"\[LeftBracketingBar]", 
        RowBox[{
        "\[LeftAngleBracket]", "\[ScriptCapitalB]", "\[RightAngleBracket]"}], 
        "\[RightBracketingBar]"}]}]}]}]}]}], 
