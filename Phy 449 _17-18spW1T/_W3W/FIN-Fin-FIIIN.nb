  "255a5324-eb0b-43e2-95c1-ac269f4573e2"],
 StyleBox["\n\n", "Subsubsection",
  FontSize->18,
  FontColor->GrayLevel[0]],
 StyleBox["We first assume that the effect of the perturbation is small, so \
that ",
  FontSize->18,
  FontColor->GrayLevel[0]],
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[LeftBracketingBar]", 
     SubscriptBox["c", "g"], "\[RightBracketingBar]"}], "\[TildeTilde]", 
    "1"}], TraditionalForm]],ExpressionUUID->
  "e6ef5268-8f10-4b2b-b842-72fe533966d8"],
 StyleBox[" and ",
  FontSize->18,
  FontColor->GrayLevel[0]],
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["c", "e"], "\[LessLess]", "1"}], TraditionalForm]],
  ExpressionUUID->"5980c674-e0f1-4bbc-92df-770d7afdb715"],
 StyleBox[".  (nb)\n\n",
  FontSize->18,
  FontColor->GrayLevel[0]],
 Cell[BoxData[
  RowBox[{
   RowBox[{"\[ImaginaryI]", " ", "\[HBar]", " ", 
    TemplateBox[{"e",OverscriptBox["\[Psi]", "."]},
     "BraKet"]}], " ", "=", 
   RowBox[{
    RowBox[{
     RowBox[{
      TemplateBox[{"e"},
       "Bra"], "Ha", 
      TemplateBox[{"\[Psi]"},
       "Ket"]}], "+", 
     RowBox[{
      TemplateBox[{"e"},
       "Bra"], "V", 
      RowBox[{"(", "t", ")"}], 
      TemplateBox[{"\[Psi]"},
       "Ket"]}]}], "=", 
    RowBox[{
     TemplateBox[{"e","\[Psi]"},
      "BraKet"], "+", 
     RowBox[{
      TemplateBox[{"e"},
       "Bra"], "V", 
      TemplateBox[{"\[Psi]"},
       "Ket"]}]}]}]}]],
  CellChangeTimes->{{3.734987333580481*^9, 3.734987334061098*^9}, {
   3.7349873808870697`*^9, 3.734987411927546*^9}},ExpressionUUID->
  "c663e2ac-c715-49aa-bfa1-b45ba0ae2483"],
 "\n",
 StyleBox["\n", "Subsubsection",
  FontSize->18,
  FontColor->GrayLevel[0]],
 Cell[BoxData[
  RowBox[{"\[ImaginaryI]", " ", "\[HBar]", " ", 
   RowBox[{"(", 
    RowBox[{
     RowBox[{
      FractionBox["d", "dt"], 
      SubscriptBox["c", "e"]}], "-", " ", 
     RowBox[{"\[ImaginaryI]", " ", "\[Omega]0", " ", 
      SubscriptBox["c", "e"]}]}], ")"}], 
   SuperscriptBox["\[ExponentialE]", 
    RowBox[{
     RowBox[{"-", "\[ImaginaryI]"}], " ", "\[Omega]0", " ", "t"}]], " "}]],
  CellChangeTimes->{{3.734987333580481*^9, 3.734987334061098*^9}, {
   3.7349873808870697`*^9, 3.734987411927546*^9}, {3.7349874659521017`*^9, 
   3.734987499942519*^9}, {3.734987546995577*^9, 3.734987568038116*^9}, {
   3.734987618942294*^9, 3.7349876353826113`*^9}},ExpressionUUID->
  "fc9b03ad-7534-467b-b9c2-538dd2ca7ed0"],
 StyleBox[" = ", "Subsubsection",
  FontSize->18,
  FontColor->GrayLevel[0]],
 Cell[BoxData[
  RowBox[{"\[HBar]", " ", "\[Omega]0", " ", 
   SubscriptBox["c", "e"], 
   SuperscriptBox["\[ExponentialE]", 
    RowBox[{
     RowBox[{"-", "\[ImaginaryI]"}], " ", "\[Omega]0", " ", "t"}]]}]],
  CellChangeTimes->{{3.734987333580481*^9, 3.734987334061098*^9}, {
   3.7349873808870697`*^9, 3.734987411927546*^9}, {3.7349874659521017`*^9, 
   3.734987499942519*^9}, {3.734987546995577*^9, 3.734987568038116*^9}, {
   3.734987618942294*^9, 3.7349876353826113`*^9}, {3.734987665559305*^9, 
   3.73498767885844*^9}, {3.734987731945315*^9, 3.73498773321504*^9}},
  ExpressionUUID->"447d5bdf-0931-45a6-b9f8-76a5ba9a310c"],
 " + \[LeftAngleBracket]e|V |g\[RightAngleBracket]\n\n",
 Cell[BoxData[
  RowBox[{
   RowBox[{
    FractionBox["d", "dt"], 
    SubscriptBox["c", "e"]}], "=", " ", 
   FractionBox[
    RowBox[{"\[LeftAngleBracket]", 
     RowBox[{"e", "|", "V", " ", "|", "g"}], "\[RightAngleBracket]"}], 
    RowBox[{"\[ImaginaryI]", " ", "\[HBar]", " ", 
     SuperscriptBox["\[ExponentialE]", 
      RowBox[{
       RowBox[{"-", "\[ImaginaryI]"}], " ", "\[Omega]0", " ", "t"}]]}]]}]],
  CellChangeTimes->{{3.734987931462528*^9, 3.734987931481305*^9}, {
   3.73498807697363*^9, 3.734988097603648*^9}},ExpressionUUID->
  "c86e8f81-4564-49c0-8099-49a5574ac8b4"],
 "\n\n\[LeftAngleBracket]e|V |g\[RightAngleBracket] = \[LeftAngleBracket]e|(\
\[HBar] \[Omega]r Cos[\[Omega] t]|e\[RightAngleBracket]\[LeftAngleBracket]g|+\
\[HBar] \[Omega]r Cos[\[Omega] \
t]|g\[RightAngleBracket]\[LeftAngleBracket]e|)|g\[RightAngleBracket] \n= \
\[LeftAngleBracket]e|(\[HBar] \[Omega]r Cos[\[Omega] t]|e\[RightAngleBracket]\
\n\n",
 Cell[BoxData[
  RowBox[{
   RowBox[{
    FractionBox["d", "dt"], 
    SubscriptBox["c", "e"]}], "=", " ", 
   FractionBox[
    RowBox[{
     RowBox[{"\[LeftAngleBracket]", 
      RowBox[{"e", "|", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"\[HBar]", " ", "\[Omega]r", " ", 
          RowBox[{"Cos", "[", 
           RowBox[{"\[Omega]", " ", "t"}], "]"}]}], "|", "e"}]}]}]}], 
     "\[RightAngleBracket]"}], 
    RowBox[{"\[ImaginaryI]", " ", "\[HBar]", " ", 
     SuperscriptBox["\[ExponentialE]", 
      RowBox[{
       RowBox[{"-", "\[ImaginaryI]"}], " ", "\[Omega]0", " ", "t"}]]}]]}]],
  CellChangeTimes->{{3.734988719332534*^9, 3.73498872087932*^9}},
  ExpressionUUID->"8ef7e2f9-2ff5-4d9a-b018-b7fe112c79ad"],
 "\n\n",
 Cell[BoxData[
  RowBox[{
   RowBox[{
    FractionBox["d", "dt"], 
    SubscriptBox["c", "e"]}], "=", " ", 
   FractionBox[
    RowBox[{" ", 
     RowBox[{"\[Omega]r", " ", 
      RowBox[{"Cos", "[", 
       RowBox[{"\[Omega]", " ", "t"}], "]"}]}]}], 
    RowBox[{"\[ImaginaryI]", "  ", 
     SuperscriptBox["\[ExponentialE]", 
      RowBox[{
       RowBox[{"-", "\[ImaginaryI]"}], " ", "\[Omega]0", " ", "t"}]]}]]}]],
  CellChangeTimes->{{3.734988719332534*^9, 3.734988734907448*^9}},
  ExpressionUUID->"f8e51920-b3cc-4276-bc28-ea97b23b0c1c"],
 "\n\n",
 Cell[BoxData[
  RowBox[{
   RowBox[{
    FractionBox["d", "dt"], 
    SubscriptBox["c", "e"]}], "=", "  ", 
   RowBox[{
    RowBox[{"-", "\[ImaginaryI]"}], " ", "\[Omega]r", " ", 
    FractionBox["1", "2"], 
    RowBox[{"(", 
     RowBox[{
      SuperscriptBox["\[ExponentialE]", 
       RowBox[{"\[ImaginaryI]", " ", "\[Omega]t", " "}]], "+", " ", 
