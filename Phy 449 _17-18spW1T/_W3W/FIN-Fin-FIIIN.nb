      RowBox[{"I", " ", 
       RowBox[{"(", 
        RowBox[{"I", "+", "1"}], ")"}]}], " ", "+", " ", 
      RowBox[{"J", 
       RowBox[{"(", 
        RowBox[{"J", "+", "1"}], ")"}]}]}], 
     RowBox[{"2", " ", "f", 
