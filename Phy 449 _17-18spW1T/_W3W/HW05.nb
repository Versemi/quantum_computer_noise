Cell["first find hyperfine splittings for 1s,2s states", "Text",
 CellChangeTimes->{{3.6966812510491734`*^9, 3.696681301864271*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"s", "=", 
   RowBox[{"angmom", "[", 
    RowBox[{"1", "/", "2"}], "]"}]}], ";", 
  RowBox[{"i", "=", 
   RowBox[{"angmom", "[", 
    RowBox[{"1", "/", "2"}], "]"}]}], ";", 
  RowBox[{"With", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"psi0", "=", 
      RowBox[{
       RowBox[{"Simplify", "[", 
        RowBox[{
         FractionBox[
          RowBox[{"{", 
           RowBox[{
            RowBox[{
             SubscriptBox["P", 
              RowBox[{"1", ",", "0"}]], "[", "ss", "]"}], ",", 
            RowBox[{
             SubscriptBox["P", 
              RowBox[{"2", ",", "0"}]], "[", "ss", "]"}]}], "}"}], "ss"], 
         FractionBox["1", 
          SqrtBox[
           RowBox[{"4", "\[Pi]", " ", 
            SuperscriptBox[
             SubscriptBox["a", "0"], "3"]}]]]}], "]"}], "/.", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"ss", "\[Rule]", "0"}], ",", 
         RowBox[{
          SubscriptBox["a", "0"], "\[Rule]", " ", 
          RowBox[{"0.5292", " ", "\[CapitalARing]"}]}]}], "}"}]}]}], "}"}], 
    ",", 
    RowBox[{"As", "=", 
     RowBox[{
      RowBox[{
       RowBox[{
        FractionBox[
         RowBox[{"8", "\[Pi]", " ", 
          SubscriptBox["g", "p"], 
          SubscriptBox["g", "S"], " "}], "3"], 
        FractionBox[
         SubscriptBox["m", "e"], 
         SubscriptBox["m", "p"]], 
        SuperscriptBox[
         RowBox[{"(", 
          FractionBox[
           RowBox[{"e", " ", "\[HBar]c"}], 
           RowBox[{"2", " ", "mcsq"}]], ")"}], "2"], 
        SuperscriptBox["psi0", "2"]}], "//.", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{
          SubscriptBox["g", "p"], "\[Rule]", " ", "5.586"}], ",", 
         RowBox[{
          SubscriptBox["g", "S"], "\[Rule]", "  ", "2"}], ",", 
         RowBox[{
          FractionBox[
           SubscriptBox["m", "e"], 
           SubscriptBox["m", "p"]], "\[Rule]", " ", 
          FractionBox[".511", "938.28"]}], ",", 
         RowBox[{"e", "\[Rule]", 
          SqrtBox[
           RowBox[{"14.4", " ", "eV", " ", "\[CapitalARing]"}]]}], ",", 
         RowBox[{"\[HBar]c", "\[Rule]", 
          RowBox[{"1973", " ", "eV", " ", "\[CapitalARing]"}]}], ",", 
         RowBox[{"mcsq", "\[Rule]", 
          RowBox[{"511000", " ", "eV"}]}]}], "}"}]}], "//", 
      "PowerExpand"}]}]}], "]"}]}]], "Input",
