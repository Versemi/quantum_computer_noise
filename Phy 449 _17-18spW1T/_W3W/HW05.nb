Cell[TextData[{
 "2) Two Ca atoms in the ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox[" \[VeryThinSpace]", "1"], "\[VeryThinSpace]", 
    SubscriptBox["S", "0"]}], TraditionalForm]]],
 " state attract each other with a van der Waals interaction ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"U", "(", "R", ")"}], "=", 
    RowBox[{"-", 
     FractionBox[
      SubscriptBox["c", "6"], 
      SuperscriptBox["R", "6"]]}]}], TraditionalForm]]],
 ".  Assuming the van der Waals interaction comes about primarily due to 2nd \
order matrix elements to the  ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox[" \[VeryThinSpace]", "1"], "\[VeryThinSpace]", 
    SubscriptBox["P", "1"]}], TraditionalForm]]],
 " excited states with energy ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["E", "p"], TraditionalForm]]],
 ", calculate ",
 Cell[BoxData[
  FormBox[
   RowBox[{"U", 
    RowBox[{"(", "R", ")"}]}], TraditionalForm]]],
 " when the Ca atoms are both in the  ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox[" \[VeryThinSpace]", "1"], "\[VeryThinSpace]", 
    SubscriptBox["P", "1"]}], TraditionalForm]]],
 " state."
}], "Subsubsection",
 CellChangeTimes->{{3.606050686942819*^9, 3.606050689653903*^9}, {
   3.633044423576584*^9, 3.633044444259316*^9}, 3.6649928379427643`*^9, 
   3.6649929213173857`*^9, {3.664993054720202*^9, 3.664993093036891*^9}, {
