Cell[TextData[{
 "First we break the Hamiltonian into two parts\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"H", "=", 
    RowBox[{
     SubscriptBox["H", "0"], "+", 
     RowBox[{"V", "(", "t", ")"}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "8494f1da-6ff5-4b6e-be83-b65daec94e6d"],
 "  where ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["H", "0"], "=", 
    RowBox[{"\[HBar]", " ", 
     SubscriptBox["\[Omega]", "0"], 
     TemplateBox[{"e"},
      "Ket"], 
     TemplateBox[{"e"},
      "Bra"]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "32897e6d-6122-4127-83c1-b8a151547758"],
 "\nNote the energies of the two states are\n",
 Cell[BoxData[{
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["H", "0"], 
     TemplateBox[{"e"},
      "Ket"]}], "=", 
    RowBox[{"\[HBar]", " ", 
     SubscriptBox["\[Omega]", "0"], 
     TemplateBox[{"e"},
      "Ket"]}]}], TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["H", "0"], 
     TemplateBox[{"g"},
      "Ket"]}], "=", 
    RowBox[{"0", 
     TemplateBox[{"g"},
      "Ket"]}]}], TraditionalForm]}],
  FormatType->"TraditionalForm",ExpressionUUID->
  "7b46a55f-c46d-40c2-b341-01dea9311db0"],
 "\nNow we assume the following wavefunction and apply time-dependent \
perturbation theory\n",
 Cell[BoxData[{
  FormBox[
   RowBox[{
    TemplateBox[{"\[Psi]"},
     "Ket"], "=", 
    RowBox[{
     RowBox[{
      SuperscriptBox["\[ExponentialE]", 
       RowBox[{
        RowBox[{"-", "\[ImaginaryI]"}], " ", "0", " ", 
        RowBox[{"t", "/", "\[HBar]"}]}]], 
      TemplateBox[{"g"},
       "Ket"]}], "+", 
     RowBox[{
      SubscriptBox["c", "e"], 
      SuperscriptBox["\[ExponentialE]", 
       RowBox[{
        RowBox[{"-", "\[ImaginaryI]"}], " ", "\[HBar]", " ", 
        SubscriptBox["\[Omega]", "0"], " ", 
        RowBox[{"t", "/", "\[HBar]"}]}]], 
      TemplateBox[{"e"},
       "Ket"]}]}]}], TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"=", 
    RowBox[{
     TemplateBox[{"g"},
      "Ket"], "+", 
     RowBox[{
      SubscriptBox["c", "e"], 
      SuperscriptBox["\[ExponentialE]", 
       RowBox[{
        RowBox[{"-", "\[ImaginaryI]"}], " ", 
        SubscriptBox["\[Omega]", "0"], " ", "t"}]], 
      TemplateBox[{"e"},
       "Ket"]}]}]}], TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{
    RowBox[{"\[Implies]", 
     RowBox[{"\[ImaginaryI]", " ", "\[HBar]", 
      TemplateBox[{"e",OverscriptBox["\[Psi]", "."]},
       "BraKet"]}]}], "=", 
    RowBox[{
     RowBox[{
      TemplateBox[{"e"},
       "Bra"], 
      SubscriptBox["H", "0"], 
      TemplateBox[{"\[Psi]"},
       "Ket"]}], "+", 
     RowBox[{
      TemplateBox[{"e"},
       "Bra"], "V", 
      TemplateBox[{"\[Psi]"},
       "Ket"]}]}]}], TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{
    RowBox[{"\[Implies]", 
     RowBox[{"\[ImaginaryI]", " ", "\[HBar]", " ", 
      RowBox[{"(", 
       RowBox[{
        SubscriptBox[
         OverscriptBox["c", "."], "e"], "-", 
        RowBox[{"\[ImaginaryI]", " ", 
         SubscriptBox["\[Omega]", "0"], 
         SubscriptBox["c", "e"]}]}], ")"}], 
      SuperscriptBox["\[ExponentialE]", 
       RowBox[{
        RowBox[{"-", "\[ImaginaryI]"}], " ", 
        SubscriptBox["\[Omega]", "0"], " ", "t"}]]}]}], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["c", "e"], "\[HBar]", " ", 
      SubscriptBox["\[Omega]", "0"], 
      SuperscriptBox["\[ExponentialE]", 
       RowBox[{
        RowBox[{"-", "\[ImaginaryI]"}], " ", 
        SubscriptBox["\[Omega]", "0"], " ", "t"}]]}], "+", 
     RowBox[{
      TemplateBox[{"e"},
       "Bra"], "V", 
      TemplateBox[{"g"},
       "Ket"]}]}]}], TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{
    RowBox[{"\[Implies]", 
     SubscriptBox[
      OverscriptBox["c", "."], "e"]}], "=", 
    RowBox[{
     FractionBox[
      SuperscriptBox["\[ExponentialE]", 
       RowBox[{"\[ImaginaryI]", " ", 
        SubscriptBox["\[Omega]", "0"], " ", "t"}]], 
      RowBox[{"\[ImaginaryI]", " ", "\[HBar]"}]], 
     TemplateBox[{"e"},
      "Bra"], "V", 
     TemplateBox[{"g"},
      "Ket"]}]}], TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{
    RowBox[{"\[Implies]", 
     SubscriptBox[
      OverscriptBox["c", "."], "e"]}], "=", 
    RowBox[{
     FractionBox[
      SuperscriptBox["\[ExponentialE]", 
       RowBox[{"\[ImaginaryI]", " ", 
        SubscriptBox["\[Omega]", "0"], " ", "t"}]], 
      RowBox[{"\[ImaginaryI]", " ", "\[HBar]"}]], 
     RowBox[{
      TemplateBox[{"e"},
       "Bra"], "(", 
      RowBox[{
       RowBox[{"\[HBar]", " ", 
        SubscriptBox["\[Omega]", "r"], 
        RowBox[{"Cos", "[", 
         RowBox[{"\[Omega]", " ", "t"}], "]"}], 
        TemplateBox[{"e"},
         "Ket"], 
        TemplateBox[{"g"},
         "Bra"]}], "+", 
       RowBox[{"\[HBar]", " ", 
        SubscriptBox["\[Omega]", "r"], 
        RowBox[{"Cos", "[", 
         RowBox[{"\[Omega]", " ", "t"}], "]"}], 
        TemplateBox[{"g"},
         "Ket"], 
        TemplateBox[{"e"},
         "Bra"]}]}], ")"}], 
     TemplateBox[{"g"},
      "Ket"]}]}], TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{
    RowBox[{"\[Implies]", 
     SubscriptBox[
      OverscriptBox["c", "."], "e"]}], "=", 
    RowBox[{
     FractionBox[
      SuperscriptBox["\[ExponentialE]", 
       RowBox[{"\[ImaginaryI]", " ", 
        SubscriptBox["\[Omega]", "0"], " ", "t"}]], 
      RowBox[{"\[ImaginaryI]", " ", "\[HBar]"}]], 
     TemplateBox[{"e"},
      "Bra"], "\[HBar]", " ", 
     SubscriptBox["\[Omega]", "r"], 
     RowBox[{"Cos", "[", 
      RowBox[{"\[Omega]", " ", "t"}], "]"}], 
     TemplateBox[{"e"},
      "Ket"]}]}], TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{
    RowBox[{"\[Implies]", 
     SubscriptBox[
      OverscriptBox["c", "."], "e"]}], "=", 
    RowBox[{
     RowBox[{"-", "\[ImaginaryI]"}], " ", 
     SubscriptBox["\[Omega]", "r"], 
     RowBox[{"Cos", "[", 
      RowBox[{"\[Omega]", " ", "t"}], "]"}], 
     SuperscriptBox["\[ExponentialE]", 
      RowBox[{"\[ImaginaryI]", " ", 
       SubscriptBox["\[Omega]", "0"], " ", "t"}]]}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{
    RowBox[{"\[Implies]", 
     SubscriptBox[
      OverscriptBox["c", "."], "e"]}], "=", 
    RowBox[{
     RowBox[{"-", "\[ImaginaryI]"}], " ", 
     RowBox[{
      SubscriptBox["\[Omega]", "r"], "(", 
      RowBox[{
       RowBox[{
        FractionBox["1", "2"], " ", 
        SuperscriptBox["\[ExponentialE]", 
         RowBox[{
          RowBox[{"-", "\[ImaginaryI]"}], " ", "t", " ", "\[Omega]"}]]}], "+", 
       RowBox[{
        FractionBox["1", "2"], " ", 
        SuperscriptBox["\[ExponentialE]", 
         RowBox[{"\[ImaginaryI]", " ", "t", " ", "\[Omega]"}]]}]}], ")"}], 
     SuperscriptBox["\[ExponentialE]", 
      RowBox[{"\[ImaginaryI]", " ", 
       SubscriptBox["\[Omega]", "0"], " ", "t"}]]}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{
    RowBox[{"\[Implies]", 
     SubscriptBox[
      OverscriptBox["c", "."], "e"]}], "=", 
    RowBox[{
     RowBox[{"-", "\[ImaginaryI]"}], " ", 
     SubscriptBox["\[Omega]", "r"], 
     FractionBox["1", "2"], " ", 
     RowBox[{"(", 
      RowBox[{
       SuperscriptBox["\[ExponentialE]", 
        RowBox[{
         RowBox[{"-", "\[ImaginaryI]"}], " ", "t", " ", 
         RowBox[{"(", 
          RowBox[{"\[Omega]", "-", 
           SubscriptBox["\[Omega]", "0"]}], ")"}]}]], "+", 
       SuperscriptBox["\[ExponentialE]", 
        RowBox[{"\[ImaginaryI]", " ", "t", " ", 
         RowBox[{"(", 
          RowBox[{"\[Omega]", "+", 
           SubscriptBox["\[Omega]", "0"]}], ")"}]}]]}], ")"}]}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"Using", " ", "rotating", " ", "wave", " ", "approximation"}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{
    RowBox[{"\[Implies]", 
     SubscriptBox[
      OverscriptBox["c", "."], "e"]}], "=", 
    RowBox[{
     RowBox[{"-", "\[ImaginaryI]"}], " ", 
     SubscriptBox["\[Omega]", "r"], 
     FractionBox["1", "2"], " ", 
     SuperscriptBox["\[ExponentialE]", 
      RowBox[{
       RowBox[{"-", "\[ImaginaryI]"}], " ", "t", " ", 
       RowBox[{"(", 
        RowBox[{"\[Omega]", "-", 
         SubscriptBox["\[Omega]", "0"]}], ")"}]}]]}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{
    RowBox[{"\[Implies]", 
     SubscriptBox["c", "e"]}], "=", 
    RowBox[{
     SubsuperscriptBox["\[Integral]", "0", "t"], " ", 
     RowBox[{
      FractionBox[
       SubscriptBox["\[Omega]", "r"], 
       RowBox[{"2", "\[ImaginaryI]"}]], " ", 
      SuperscriptBox["\[ExponentialE]", 
       RowBox[{
        RowBox[{"-", "\[ImaginaryI]"}], " ", 
        RowBox[{"t", "'"}], " ", 
        RowBox[{"(", 
         RowBox[{"\[Omega]", "-", 
          SubscriptBox["\[Omega]", "0"]}], ")"}]}]], 
      RowBox[{"dt", "'"}]}]}]}], TraditionalForm]}],
  FormatType->"TraditionalForm",ExpressionUUID->
  "76ff4fe4-3a41-492b-af34-4e5da69952bc"]
}], "Text",
