Cell[TextData[{
 "To calculate the diamagnetic energy shift\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["H", "dia"], "=", 
    RowBox[{
     FractionBox[
      RowBox[{
       SuperscriptBox["e", "2"], 
       SuperscriptBox["B", "2"]}], 
      RowBox[{"2", "m", " ", 
       SuperscriptBox["c", "2"]}]], 
     FractionBox["1", "4"], 
     RowBox[{"\[LeftAngleBracket]", 
      RowBox[{
       SuperscriptBox["x", "2"], "+", 
       SuperscriptBox["y", "2"]}], "\[RightAngleBracket]"}]}]}], 
   TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "439930bd-8b70-4576-830b-784a29f4e288"],
 "\nwe use first order perturbation theory\n",
 Cell[BoxData[{
  FormBox[
   RowBox[{
    SubsuperscriptBox["E", "0", 
     RowBox[{"(", "1", ")"}]], "=", 
    RowBox[{
     RowBox[{
      FractionBox[
       RowBox[{
        SuperscriptBox["e", "2"], 
        SuperscriptBox["B", "2"]}], 
       RowBox[{"2", "m", " ", 
        SuperscriptBox["c", "2"]}]], 
      FractionBox["1", "4"], 
      TemplateBox[{SubsuperscriptBox["\[Phi]", "0", 
         RowBox[{"(", "0", ")"}]]},
       "Bra"], 
      SuperscriptBox["x", "2"]}], "+", 
     RowBox[{
      SuperscriptBox["y", "2"], 
      TemplateBox[{SubsuperscriptBox["\[Phi]", "0", 
         RowBox[{"(", "0", ")"}]]},
       "Ket"]}]}]}], TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"=", 
    RowBox[{
     RowBox[{
      FractionBox[
       RowBox[{
        SuperscriptBox["e", "2"], 
        SuperscriptBox["B", "2"]}], 
       RowBox[{"2", "m", " ", 
        SuperscriptBox["c", "2"]}]], 
      FractionBox["1", "4"], 
      TemplateBox[{RowBox[{"1", "s1s"}]},
       "Bra"], 
      SuperscriptBox["r", "2"]}], "-", 
     RowBox[{
      SuperscriptBox["z", "2"], 
      TemplateBox[{RowBox[{"1", "s1s"}]},
       "Ket"]}]}]}], TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"=", 
    RowBox[{
     RowBox[{
      FractionBox[
       RowBox[{
        SuperscriptBox["e", "2"], 
        SuperscriptBox["B", "2"]}], 
       RowBox[{"2", "m", " ", 
        SuperscriptBox["c", "2"]}]], 
      FractionBox["1", "4"], 
      TemplateBox[{RowBox[{"1", "s1s"}]},
       "Bra"], 
      SubsuperscriptBox["r", "1", "2"]}], "+", 
     SubsuperscriptBox["r", "2", "2"], "-", 
     SubsuperscriptBox["z", "1", "2"], "-", 
     RowBox[{
      SubsuperscriptBox["z", "2", "2"], 
      TemplateBox[{RowBox[{"1", "s1s"}]},
       "Ket"]}]}]}], TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"=", 
    RowBox[{
     RowBox[{
      FractionBox[
       RowBox[{
        SuperscriptBox["e", "2"], 
        SuperscriptBox["B", "2"]}], 
       RowBox[{"2", "m", " ", 
        SuperscriptBox["c", "2"]}]], 
      FractionBox["1", "4"], 
      TemplateBox[{RowBox[{"1", "s1s"}]},
       "Bra"], 
      SubsuperscriptBox["r", "1", "2"]}], "+", 
     SubsuperscriptBox["r", "2", "2"], "-", 
     RowBox[{
      SubsuperscriptBox["r", "1", "2"], 
      SuperscriptBox[
       RowBox[{"Cos", "[", 
        SubscriptBox["\[Theta]", "1"], "]"}], "2"]}], "-", 
     RowBox[{
      SubsuperscriptBox["r", "2", "2"], 
      SuperscriptBox[
       RowBox[{"Cos", "[", 
        SubscriptBox["\[Theta]", "2"], "]"}], "2"], 
      TemplateBox[{RowBox[{"1", "s1s"}]},
       "Ket"]}]}]}], TraditionalForm]}],
  FormatType->"TraditionalForm",ExpressionUUID->
  "a2e182d1-69a1-4f4f-8b76-66a2773e4214"]
}], "Text",
