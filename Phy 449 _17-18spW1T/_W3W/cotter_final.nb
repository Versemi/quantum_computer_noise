Cell[TextData[{
 "The energy of a H atom state is\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["E", "n"], "=", 
    RowBox[{"-", 
     FractionBox[
      RowBox[{"13.6", "eV"}], 
      SuperscriptBox["n", "2"]]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "27790c3d-d0a2-4440-ab0b-0f72ac842dc1"],
 "\nThe difference between states 50 and 51 is\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[CapitalDelta]E", "50"], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"-", 
       FractionBox[
        RowBox[{"13.6", "eV"}], 
        SuperscriptBox["51", "2"]]}], "+", 
      FractionBox[
       RowBox[{"13.6", "eV"}], 
       SuperscriptBox["50", "2"]]}], "=", 
     RowBox[{"0.00021124", "eV"}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "06220d19-a3ed-4a9a-99fa-869e3588367b"]
}], "Text",
 CellChangeTimes->{{3.7029862141924753`*^9, 3.702986355366188*^9}, {
