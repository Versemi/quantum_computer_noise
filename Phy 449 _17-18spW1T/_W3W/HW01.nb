Cell[TextData[{
 "9) Find the energies and eigenstates for motion in the potential ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"V", "(", 
     RowBox[{"x", ",", "y"}], ")"}], "=", 
    RowBox[{"\[Piecewise]", GridBox[{
       {
        RowBox[{
         FractionBox["1", "2"], "\[Kappa]", " ", 
         SuperscriptBox["x", "2"]}], 
        RowBox[{"0", "<", "y", "<", "a"}]},
       {"\[Infinity]", "Elsewhere"}
      }]}]}], TraditionalForm]]],
 ".  Name your two quantum numbers ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["n", "x"], " ", "and", " ", 
    SubscriptBox["n", "y"]}], TraditionalForm]]],
 ".  Plot the lowest 10 energy levels for the case ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox[
     RowBox[{"m", " ", 
      SuperscriptBox["a", "4"], "\[Kappa]"}], 
     SuperscriptBox["\[HBar]", "2"]], "=", 
    FractionBox[
     SuperscriptBox["\[Pi]", "4"], "4"]}], TraditionalForm]]],
 "and be sure to note any degeneracies."
}], "Subsubsection",
 CellChangeTimes->{{3.662664452077106*^9, 3.662664462381837*^9}, {
   3.662664724076356*^9, 3.662664741740511*^9}, {3.69367715686026*^9, 
   3.693677157322158*^9}, {3.6936772304816427`*^9, 3.693677230874557*^9}, 
   3.693907413095418*^9}],

Cell[TextData[{
 "This is of the right form: ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["V", "x"], "=", 
    RowBox[{
     FractionBox["1", "2"], "\[Kappa]", " ", 
     SuperscriptBox["x", "2"]}]}], TraditionalForm]]],
 ", ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["V", "y"], "=", 
    RowBox[{"\[Piecewise]", 
     RowBox[{GridBox[{
        {"0", 
         RowBox[{"0", "<", "y", "<", "a"}]},
        {"\[Infinity]", "Elsewhere"}
       }], "."}]}]}], TraditionalForm]]],
 "  ",
 Cell[BoxData[
  FormBox[
   RowBox[{"Thus", " ", Cell[TextData[Cell[BoxData[
     FormBox[
      RowBox[{"E", " ", "=", 
       RowBox[{
        RowBox[{
         RowBox[{
          SqrtBox[
           RowBox[{
            SuperscriptBox["\[HBar]", "2"], 
            FractionBox["\[Kappa]", "m"]}]], 
          RowBox[{"(", 
           RowBox[{
            SubscriptBox["n", "x"], "+", 
            FractionBox["1", "2"]}], ")"}]}], "+", 
         RowBox[{
          FractionBox[
           RowBox[{
            SuperscriptBox["\[HBar]", "2"], 
            SuperscriptBox["\[Pi]", "2"]}], 
           RowBox[{"2", "m", " ", 
            SuperscriptBox["a", "2"]}]], 
          SubsuperscriptBox["n", "y", "2"]}]}], "=", 
        RowBox[{
         RowBox[{
          FractionBox[
           RowBox[{
            SuperscriptBox["\[HBar]", "2"], 
            SuperscriptBox["\[Pi]", "2"]}], 
           RowBox[{"2", "m", " ", 
            SuperscriptBox["a", "2"]}]], 
          RowBox[{"(", 
           RowBox[{
            SubsuperscriptBox["n", "y", "2"], "+", 
            RowBox[{
             SqrtBox[
              FractionBox[
               RowBox[{"4", " ", 
                SuperscriptBox["a", "4"], " ", "m", " ", "\[Kappa]"}], 
               RowBox[{
                SuperscriptBox["\[Pi]", "4"], " ", 
                SuperscriptBox["\[HBar]", "2"]}]]], 
             RowBox[{"(", 
              RowBox[{
               SubscriptBox["n", "x"], "+", 
               FractionBox["1", "2"]}], ")"}]}]}], ")"}]}], "=", 
         RowBox[{
          FractionBox[
           RowBox[{
            SuperscriptBox["\[HBar]", "2"], 
            SuperscriptBox["\[Pi]", "2"]}], 
           RowBox[{"2", "m", " ", 
            SuperscriptBox["a", "2"]}]], 
          RowBox[{"(", 
           RowBox[{
            SubsuperscriptBox["n", "y", "2"], "+", 
            SubscriptBox["n", "x"], "+", 
            FractionBox["1", "2"]}], ")"}]}]}]}]}], TraditionalForm]]]]]}], 
   TraditionalForm]]]
}], "Text",
