  FontColor->GrayLevel[0]],
 Cell[BoxData[
  RowBox[{"\[ImaginaryI]", " ", "\[HBar]", " ", 
   RowBox[{"(", 
    RowBox[{
     RowBox[{
      FractionBox["d", "dt"], 
      SubscriptBox["c", "e"]}], "-", " ", 
     RowBox[{"\[ImaginaryI]", " ", "\[Omega]0", " ", 
      SubscriptBox["c", "e"]}]}], ")"}], 
   SuperscriptBox["\[ExponentialE]", 
    RowBox[{
     RowBox[{"-", "\[ImaginaryI]"}], " ", "\[Omega]0", " ", "t"}]], " "}]],
  CellChangeTimes->{{3.734987333580481*^9, 3.734987334061098*^9}, {
   3.7349873808870697`*^9, 3.734987411927546*^9}, {3.7349874659521017`*^9, 
