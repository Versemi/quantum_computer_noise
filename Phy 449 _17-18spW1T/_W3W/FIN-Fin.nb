  FontColor->GrayLevel[0]],
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[LeftBracketingBar]", 
     SubscriptBox["c", "g"], "\[RightBracketingBar]"}], "\[TildeTilde]", 
    "1"}], TraditionalForm]],ExpressionUUID->
  "e6ef5268-8f10-4b2b-b842-72fe533966d8"],
 StyleBox[" and ",
  FontSize->18,
  FontColor->GrayLevel[0]],
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["c", "e"], "\[LessLess]", "1"}], TraditionalForm]],
  ExpressionUUID->"5980c674-e0f1-4bbc-92df-770d7afdb715"],
 StyleBox[".  (nb)\n\n",
  FontSize->18,
  FontColor->GrayLevel[0]],
 Cell[BoxData[
  RowBox[{
   RowBox[{"\[ImaginaryI]", " ", "\[HBar]", " ", 
    TemplateBox[{"e",OverscriptBox["\[Psi]", "."]},
     "BraKet"]}], " ", "=", 
   RowBox[{
    RowBox[{
     RowBox[{
      TemplateBox[{"e"},
       "Bra"], "Ha", 
      TemplateBox[{"\[Psi]"},
       "Ket"]}], "+", 
     RowBox[{
      TemplateBox[{"e"},
       "Bra"], "V", 
      RowBox[{"(", "t", ")"}], 
      TemplateBox[{"\[Psi]"},
       "Ket"]}]}], "=", 
    RowBox[{
     TemplateBox[{"e","\[Psi]"},
      "BraKet"], "+", 
     RowBox[{
      TemplateBox[{"e"},
       "Bra"], "V", 
      TemplateBox[{"\[Psi]"},
       "Ket"]}]}]}]}]],
  CellChangeTimes->{{3.734987333580481*^9, 3.734987334061098*^9}, {
