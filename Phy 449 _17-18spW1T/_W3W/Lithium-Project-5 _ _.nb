      TemplateBox[{RowBox[{"a", ",", "b", ",", "c"}]},
       "Bra"], 
      OverscriptBox[
       RowBox[{"H", " "}], "^"], 
      TemplateBox[{RowBox[{"B", ",", "C", ",", "A"}]},
       "Ket"], 
      TemplateBox[{
       RowBox[{"d", ",", "u", ",", "u"}],RowBox[{"u", ",", "u", ",", "d"}]},
       "BraKet"]}], "-", 
     RowBox[{
      TemplateBox[{RowBox[{"a", ",", "b", ",", "c"}]},
       "Bra"], 
      OverscriptBox[
       RowBox[{"H", " "}], "^"], 
      TemplateBox[{RowBox[{"B", ",", "A", ",", "C"}]},
       "Ket"], 
      TemplateBox[{
       RowBox[{"d", ",", "u", ",", "u"}],RowBox[{"u", ",", "d", ",", "u"}]},
       "BraKet"]}], "+", 
     RowBox[{
      TemplateBox[{RowBox[{"a", ",", "b", ",", "c"}]},
       "Bra"], 
      OverscriptBox[
       RowBox[{"H", " "}], "^"], 
      TemplateBox[{RowBox[{"C", ",", "A", ",", "B"}]},
       "Ket"], 
      TemplateBox[{
       RowBox[{"d", ",", "u", ",", "u"}],RowBox[{"u", ",", "d", ",", "u"}]},
