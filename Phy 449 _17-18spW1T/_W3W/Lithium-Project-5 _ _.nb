600e2386a6bb"],

Cell[TextData[{
 "Let ",
 Cell[BoxData[
  FormBox[
   RowBox[{"Q", "=", 
    RowBox[{
     SubscriptBox["P", "23"], 
     SubscriptBox["P", "12"]}]}], TraditionalForm]],ExpressionUUID->
  "3ac6aa0c-0c2d-41e4-9be7-f0ac580afb33"],
 "\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"Q", 
     TemplateBox[{"\[Psi]"},
      "Ket"]}], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox[
       SubscriptBox["P", "23"], "P12"], 
      TemplateBox[{"\[Psi]"},
       "Ket"]}], "=", 
     RowBox[{
      RowBox[{
       SubscriptBox["P", "23"], "(", 
       RowBox[{"-", 
        TemplateBox[{"\[Psi]"},
         "Ket"]}], ")"}], "=", 
      RowBox[{
       RowBox[{"-", 
        RowBox[{"(", 
         RowBox[{"-", 
          TemplateBox[{"\[Psi]"},
           "Ket"]}], ")"}]}], "=", 
       TemplateBox[{"\[Psi]"},
        "Ket"]}]}]}]}], TraditionalForm]],ExpressionUUID->
  "3c5c1b60-10c2-4168-a15d-eee95764b5a1"],
