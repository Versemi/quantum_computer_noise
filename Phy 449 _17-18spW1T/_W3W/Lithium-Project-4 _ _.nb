  "402ce962-b7a5-4db2-98d8-5f9d4a80b283"],
 " because commutation relations and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"[", 
     RowBox[{
      SubscriptBox["P", "ij"], ",", 
      OverscriptBox["H", "^"]}], "]"}], "=", "0"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "cd6c2ee6-fcd0-444b-9fef-e24416c7c2c3"]
}], "Text",
 CellChangeTimes->{{3.728759860741804*^9, 3.7287599302468224`*^9}, {
  3.728760277157105*^9, 3.7287603919608393`*^9}, {3.728760479521147*^9, 
  3.728760479873176*^9}},ExpressionUUID->"9650dbe7-45e0-4bbc-8c40-\
45f418b8a6d5"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"[", 
     RowBox[{
      OverscriptBox["\[ScriptCapitalR]", "^"], ",", 
      OverscriptBox["H", "^"]}], "]"}], "=", 
    RowBox[{
     RowBox[{"[", 
      RowBox[{
       RowBox[{
        SubscriptBox["P", "23"], 
        SubscriptBox["P", "13"]}], ",", 
       OverscriptBox["H", "^"]}], "]"}], "=", 
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{"[", 
         RowBox[{
          SubscriptBox["P", "23"], ",", 
          OverscriptBox["H", "^"]}], "]"}], 
        SubscriptBox["P", "13"]}], "+", 
       RowBox[{
        SubscriptBox["P", "23"], "[", 
        RowBox[{
         SubscriptBox["P", "13"], ",", 
         OverscriptBox["H", "^"]}], "]"}]}], "=", "0"}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "26511403-adb8-4a55-b48a-9c94dffcdf84"],
 " because commutation relations and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"[", 
     RowBox[{
      SubscriptBox["P", "ij"], ",", 
      OverscriptBox["H", "^"]}], "]"}], "=", "0"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "ba2d7afd-ce7f-4261-9686-99255a4121c0"]
}], "Text",
 CellChangeTimes->{{3.7287604275876217`*^9, 
  3.728760481616081*^9}},ExpressionUUID->"fe876e3c-8ed7-4374-bcde-\
7abffc60da20"],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    SuperscriptBox[
     OverscriptBox["\[ScriptCapitalL]", "^"], "3"], 
    TemplateBox[{RowBox[{"a", ";", "b", ";", "c"}]},
     "Ket"]}], "=", 
   RowBox[{
    RowBox[{
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{
        SubscriptBox["P", "13"], 
        SubscriptBox["P", "23"]}], ")"}], "3"], 
     TemplateBox[{RowBox[{"a", ";", "b", ";", "c"}]},
      "Ket"]}], "=", 
    RowBox[{
     RowBox[{
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{
         SubscriptBox["P", "13"], 
         SubscriptBox["P", "23"]}], ")"}], "2"], 
      TemplateBox[{RowBox[{"b", ";", "c", ";", "a"}]},
       "Ket"]}], "=", 
     RowBox[{
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         SubscriptBox["P", "13"], 
         SubscriptBox["P", "23"]}], ")"}], 
       TemplateBox[{RowBox[{"c", ";", "a", ";", "b"}]},
        "Ket"]}], "=", 
      RowBox[{"1", 
       TemplateBox[{RowBox[{"a", ";", "b", ";", "c"}]},
        "Ket"]}]}]}]}]}], TraditionalForm]],
 FormatType->
  "TraditionalForm",ExpressionUUID->"ab1432d3-da59-4217-815a-cd22f3b57735"]], \
"Text",
 CellChangeTimes->{{3.728756651772429*^9, 3.728756763150474*^9}, {
